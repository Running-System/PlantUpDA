<file path="PlantUp/src/stores/UserStore.ts">
  1: import { create } from 'zustand';
  2: import { User } from '@supabase/supabase-js';
  3: import { db } from '../utils/supabase';
  4: import { getLevelFromXP } from '../utils/leveling';
  5: 
  6: type UserProfile = {
  7:   xp: number;
  8:   level: number;
  9:   username: string;
 10:   plant_coins: number;
 11: };
 12: 
 13: type State = {
 14:   user: User | null;
 15:   profile: UserProfile | null;
 16:   setUser: (user: User | null) => void;
 17:   setProfile: (profile: UserProfile | null) => void;
 18:   addXP: (amount: number) => Promise<void>;
 19:   fetchProfile: () => Promise<void>;
 20:   addPlantCoins: (coins: number) => Promise<void>;
 21: };
 22: 
 23: export const useUserStore = create<State>((set, get) => ({
 24:   user: null,
 25:   profile: null,
 26:   setUser: (user) => {
 27:     set({ user });
 28:     if (user) {
 29:       get().fetchProfile();
 30:     } else {
 31:       set({ profile: null });
 32:     }
 33:   },
 34:   setProfile: (profile) => set({ profile }),
 35: 
 36:   addPlantCoins: async (coins: number) => {
 37:     const { user } = get();
 38:     if (!user) return;
 39: 
 40:     const { data: userData, error: selectError } = await db.user
 41:       .from('Users')
 42:       .select('username, plant_coins')
 43:       .eq('id', user.id)
 44:       .single();
 45: 
 46:     if (selectError) {
 47:       console.error('Error loading user for coin update:', selectError);
 48:       return;
 49:     }
 50: 
 51:     const currentCoins = userData?.plant_coins ?? 0;
 52:     const newCoins = currentCoins + coins;
 53: 
 54:     // 2) Update plant_coins with the new value
 55:     const { data: updatedUser, error: updateError } = await db.user
 56:       .from('Users')
 57:       .update({ plant_coins: newCoins })
 58:       .eq('id', user.id)
 59:       .select('username, plant_coins')
 60:       .single();
 61: 
 62:     if (updateError) {
 63:       console.error('Error updating plant coins:', updateError);
 64:       return;
 65:     }
 66: 
 67:     console.log('Updated user coins:', updatedUser);
 68:   },
 69: 
 70:   fetchProfile: async () => {
 71:     const { user } = get();
 72:     if (!user) return;
 73: 
 74:     // 1. Fetch Gamification Stats
 75:     const statsPromise = db.gamification
 76:       .from('player_stats')
 77:       .select('xp, level')
 78:       .eq('user_id', user.id)
 79:       .single();
 80: 
 81:     // 2. Fetch Username from User Schema
 82:     const userPromise = db.user
 83:       .from('Users')
 84:       .select('username, plant_coins')
 85:       .eq('id', user.id)
 86:       .single();
 87: 
 88:     const [statsResult, userResult] = await Promise.all([statsPromise, userPromise]);
 89: 
 90:     console.log('Stats Result:', statsResult);
 91:     console.log('User Result:', userResult);
 92: 
 93:     const statsData = statsResult.data;
 94:     const statsError = statsResult.error;
 95:     const userData = userResult.data;
 96: 
 97:     let finalProfile: UserProfile = {
 98:       xp: 0,
 99:       level: 1,
100:       username: userData?.username || 'Gardener',
101:       plant_coins: userData?.plant_coins || 0,
102:     };
103: 
104:     console.log(finalProfile);
105:     // Handle Stats Logic (Create if missing)
106:     if (statsData) {
107:       finalProfile.xp = statsData.xp || 0;
108:       finalProfile.level = statsData.level || 1;
109:     } else if (statsError && statsError.code === 'PGRST116') {
110:       console.log('Profile stats not found, creating new player_stats...');
111:       const { data: newData } = await db.gamification
112:         .from('player_stats')
113:         .insert([{ user_id: user.id, xp: 0, level: 1 }])
114:         .select()
115:         .single();
116: 
117:       if (newData) {
118:         finalProfile.xp = newData.xp;
119:         finalProfile.level = newData.level;
120:       }
121:     }
122: 
123:     set({ profile: finalProfile });
124:   },
125: 
126:   addXP: async (amount: number) => {
127:     const { user, profile } = get();
128:     if (!user || !profile) return;
129: 
130:     const newXP = profile.xp + amount;
131:     const newLevel = getLevelFromXP(newXP);
132: 
133:     // Optimistic update
134:     set({ profile: { ...profile, xp: newXP, level: newLevel } });
135: 
136:     // Sync to DB
137:     const { error } = await db.gamification
138:       .from('player_stats')
139:       .update({ xp: newXP, level: newLevel })
140:       .eq('user_id', user.id);
141: 
142:     if (error) {
143:       console.error('Failed to update XP:', error);
144:     }
145:   },
146: }));
