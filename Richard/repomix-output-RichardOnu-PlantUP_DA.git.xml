 3: bool WiFiManager::loadCredentials()
 4: {
 5:     if (!sdManager.begin())
 6:         return false;
 7:     if (!sdManager.exists(WIFI_CONFIG_FILE))
 8:         return false;
 9: 
10:     String content = sdManager.readFile(WIFI_CONFIG_FILE);
11:     int newline = content.indexOf('\n');
12:     if (newline == -1)
13:         return false;
14: 
15:     ssid = content.substring(0, newline);
16:     password = content.substring(newline + 1);
17:     ssid.trim();
18:     password.trim();
19: 
20:     DEBUG_PRINT("[WiFiManager] Loaded SSID: ");
21:     DEBUG_PRINTLN(ssid);
22:     return !ssid.isEmpty();
23: }
24: 
25: void WiFiManager::saveCredentials(const String &ssidIn, const String &passIn)
26: {
27:     if (!sdManager.begin())
28:         return;
29:     String data = ssidIn + "\n" + passIn + "\n";
30:     sdManager.writeFile(WIFI_CONFIG_FILE, data);
31:     DEBUG_PRINTLN("[WiFiManager] Credentials saved to SD.");
32: }
33: 
34: bool WiFiManager::connectToWiFi()
35: {
36:     if (ssid.isEmpty())
37:         return false;
38: 
39:     DEBUG_PRINT("[WiFi] Connecting to ");
40:     DEBUG_PRINTLN(ssid);
41: 
42:     WiFi.mode(WIFI_STA);
43:     WiFi.begin(ssid.c_str(), password.c_str());
44:     unsigned long start = millis();
45:     while (WiFi.status() != WL_CONNECTED && millis() - start < 10000)
46:     {
47:         delay(500);
48:         DEBUG_PRINT(".");
49:     }
50:     DEBUG_PRINTLN("");
51: 
52:     return WiFi.status() == WL_CONNECTED;
53: }
54: 
55: bool WiFiManager::testConnection(const String &testSsid, const String &testPass)
56: {
57:     WiFi.mode(WIFI_STA);
58:     WiFi.begin(testSsid.c_str(), testPass.c_str());
59:     unsigned long start = millis();
60:     while (WiFi.status() != WL_CONNECTED && millis() - start < 8000)
61:     {
62:         delay(500);
63:         DEBUG_PRINT(".");
64:     }
65:     bool ok = WiFi.status() == WL_CONNECTED;
66:     WiFi.disconnect(true);
67:     DEBUG_PRINTLN(ok ? "\n‚úÖ Test OK" : "\n‚ùå Test failed");
