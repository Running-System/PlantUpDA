 24: // --- CONSTANTS ---
 25: const CACHE_KEY_PREFIX = 'plant_history_';
 26: const TIME_RANGES = {
 27:   'Last 1 Hour (5 Recs)': 5,
 28:   'Last 24 Hours (50 Recs)': 50,
 29:   'Last Week (300 Recs)': 300,
 30:   'All Time (Max 1000 Recs)': 1000,
 31: };
