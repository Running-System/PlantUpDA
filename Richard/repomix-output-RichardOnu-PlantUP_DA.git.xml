 48: export default function ShowMissionsScreen() {
 49:   const { theme } = useUnistyles();
 50:   const [quests, setQuests] = useState<UserQuest[]>([]);
 51:   const [loading, setLoading] = useState(true);
 52:   const [refreshing, setRefreshing] = useState(false);
 53: 
 54:   // --- FETCH DATA ---
 55:   const fetchQuests = useCallback(async () => {
 56:     try {
 57:       const {
 58:         data: { user },
 59:       } = await db.public.auth.getUser();
 60:       if (!user) return;
 61: 
 62:       const { data, error } = await db.gamification
 63:         .from('active_quests_with_plants')
 64:         .select('*')
 65:         .eq('user_id', user.id)
 66:         .order('status', { ascending: true });
 67: 
 68:       if (error) throw error;
 69: 
 70:       const formattedData = data.map((item: any) => ({
 71:         id: item.id,
 72:         status: item.status,
 73:         progress: item.progress,
 74:         plant_id: item.plant_id,
 75:         Plants: item.plant_name ? { nickname: item.plant_name } : null,
 76:         Quests: {
 77:           id: item.quest_id || item.id,
 78:           title: item.title,
 79:           description: item.description,
 80:           xp_reward: item.xp_reward,
 81:           target_count: item.target_count,
 82:           type: item.type,
 83:         },
 84:       }));
 85: 
 86:       setQuests(formattedData as any);
 87:     } catch (e) {
 88:       console.error('Error fetching quests:', e);
 89:     } finally {
 90:       setLoading(false);
 91:       setRefreshing(false);
 92:     }
 93:   }, []);
 94: 
 95:   useEffect(() => {
 96:     fetchQuests();
 97:   }, [fetchQuests]);
 98: 
 99:   const { addXP } = useUserStore();
100: 
101:   // --- CLAIM LOGIC ---
102:   const handleClaim = useCallback(async (userQuestId: number) => {
103:     try {
104:       setLoading(true);
105: 
106:       // Find the quest to get XP reward
107:       const questToClaim = quests.find((q) => q.id === userQuestId);
108:       if (!questToClaim) return;
109: 
110:       // 1. Add XP to User Profile
111:       await addXP(questToClaim.Quests.xp_reward);
112: 
113:       // 2. Update DB status to 'claimed'
114:       const { error } = await db.gamification
115:         .from('User_quests')
116:         .update({ status: 'claimed' })
117:         .eq('id', userQuestId);
118: 
119:       if (error) throw error;
120: 
121:       // 3. Update Local State
122:       setQuests((currentQuests) =>
123:         currentQuests.map((q) => {
124:           if (q.id === userQuestId) {
125:             return { ...q, status: 'claimed' };
126:           }
127:           return q;
128:         })
129:       );
130: 
131:       alert(`Claimed ${questToClaim.Quests.xp_reward} XP!`);
132:     } catch (e) {
133:       console.error('Error claiming:', e);
134:       alert('Fehler beim Claimen.');
135:     } finally {
136:       setLoading(false);
137:     }
138:   }, []);
139: 
140:   // Listen filtern
141:   const dailyQuests = quests.filter((q) => {
142:     if (q.Quests.type === 'daily') return true;
143:     if (q.Quests.type === 'conditional') return q.status !== 'claimed';
144:     return false;
145:   });
146: 
147:   const weeklyQuests = quests.filter((q) => q.Quests.type === 'weekly');
