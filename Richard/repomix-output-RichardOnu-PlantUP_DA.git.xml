286: 
287:   const canClaim = quest.status === 'completed';
288:   const isClaimed = quest.status === 'claimed';
289: 
290:   const plantName = quest.Plants?.nickname || 'your plant';
291:   const rawDesc = quest.Quests.description || '';
292:   const safeDescription = rawDesc.replace('{{plant_name}}', plantName);
293: 
294:   return (
295:     <View style={[styles.card, isClaimed && { opacity: 0.6 }]}>
