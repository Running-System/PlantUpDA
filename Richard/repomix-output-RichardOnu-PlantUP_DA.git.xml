<file path="PlantUp/src/features/plantstats/components/UnifiedChart.tsx">
  1: import React from 'react';
  2: import {
  3:   BarChart,
  4:   LineChart,
  5:   PieChart,
  6:   PopulationPyramid,
  7:   RadarChart,
  8: } from 'react-native-gifted-charts';
  9: import { useUnistyles } from 'react-native-unistyles';
 10: 
 11: type ChartType = 'bar' | 'line' | 'area' | 'horizontal' | 'pie' | 'donut' | 'radar' | 'pyramid';
 12: 
 13: interface UnifiedChartProps {
 14:   type: String;
 15:   data: any[]; // we allow flexibility, since each chart accepts different shapes
 16:   height?: number;
 17:   width?: number;
 18: }
 19: 
 20: export default function UnifiedChart({ type, data, height = 180, width = 260 }: UnifiedChartProps) {
 21:   const { theme } = useUnistyles();
 22: 
 23:   // --- Normalized values for charts that need {value,label} ---
 24:   const normalized = data.map((d: any, i: number) => ({
 25:     value: typeof d.value === 'number' ? d.value : Number(d) || 0,
 26:     label: d.label ?? '',
 27:     frontColor: d.frontColor ?? theme.colors.primary,
 28:   }));
 29: 
 30:   const maxValue = normalized.length ? Math.max(...normalized.map((d) => d.value)) : 1;
 31: 
 32:   const minValue = normalized.length ? Math.min(...normalized.map((d) => d.value)) : 0;
 33: 
 34:   // spacing auto-fit
 35:   const spacing = Math.max(8, width / (normalized.length + 2));
 36: 
 37:   const baseProps = {
 38:     data: normalized,
 39:     height,
 40:     width,
 41:     spacing,
 42:     initialSpacing: 12,
 43:     maxValue: maxValue + maxValue * 0.2,
 44:     minValue: Math.min(0, minValue),
 45:     barBorderRadius: 6,
 46:     hideRules: true,
 47:     xAxisThickness: 0,
 48:     yAxisThickness: 0,
 49:     yAxisTextStyle: { color: theme.colors.greyedOut },
 50:     xAxisLabelTextStyle: { color: theme.colors.greyedOut, fontSize: 10 },
 51:     disableScroll: true,
 52:   };
 53: 
 54:   // --- CHART TYPE SWITCH ---
 55:   switch (type) {
 56:     case 'bar':
 57:       return <BarChart {...baseProps} barWidth={18} frontColor={theme.colors.primary} />;
 58: 
 59:     case 'horizontal':
 60:       return <BarChart {...baseProps} horizontal barWidth={14} frontColor={theme.colors.primary} />;
 61: 
 62:     case 'line':
 63:       return <LineChart {...baseProps} color={theme.colors.primary} thickness={3} curved />;
 64: 
 65:     case 'area':
 66:       return (
 67:         <LineChart
 68:           {...baseProps}
 69:           color={theme.colors.primary}
 70:           thickness={3}
 71:           curved
 72:           areaChart
 73:           startFillColor={theme.colors.primary}
 74:           endFillColor={theme.colors.background}
 75:           startOpacity={0.4}
 76:           endOpacity={0}
 77:         />
 78:       );
 79: 
 80:     case 'pie':
 81:       return <PieChart data={data} radius={height / 2} showText />;
 82: 
 83:     case 'donut':
 84:       return <PieChart data={data} radius={height / 2} donut innerRadius={height / 4} showText />;
 85: 
 86:     case 'radar':
 87:       return (
 88:         <RadarChart data={normalized.map((d) => d.value)} maxValue={maxValue} noOfSections={5} />
 89:       );
 90: 
 91:     case 'pyramid':
 92:       return (
 93:         <PopulationPyramid
 94:           data={data.map((d: any) => ({
 95:             left: d.left,
 96:             right: d.right,
 97:             leftColor: theme.colors.primary,
 98:             rightColor: theme.colors.accent,
 99:           }))}
100:           height={height}
101:         />
102:       );
103: 
104:     default:
105:       return null;
106:   }
107: }
</file>

