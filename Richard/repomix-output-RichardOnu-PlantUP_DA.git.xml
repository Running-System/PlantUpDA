<file path="ESP32/Prototypes/BLE_client/src/App.jsx">
  1: import React, { useState, useRef, useCallback } from "react";
  2: import ConnectionPanel from "./components/ConnectionPanel";
  3: import MessagePanel from "./components/MessagePanel";
  4: 
  5: const SERVICE_UUID = "7c3b0001-1cbe-4e4e-9e8a-5b5d3e8a9f01";
  6: const CHAR_UUID = "7c3b0002-1cbe-4e4e-9e8a-5b5d3e8a9f01";
  7: 
  8: export default function App() {
  9:   const [status, setStatus] = useState("idle");
 10:   const [deviceName, setDeviceName] = useState("");
 11:   const [error, setError] = useState("");
 12:   const [messages, setMessages] = useState([]);
 13:   const charRef = useRef(null);
 14: 
 15:   const onDisconnect = () => {
 16:     setStatus("idle");
 17:     setDeviceName("");
 18:     charRef.current = null;
 19:   };
 20: 
 21:   const connectAndListen = useCallback(async () => {
 22:     try {
 23:       setError("");
 24:       setStatus("pairing");
 25: 
 26:       const device = await navigator.bluetooth.requestDevice({
 27:         filters: [{ namePrefix: "PlantUp" }],
 28:         optionalServices: [SERVICE_UUID],
 29:       });
 30: 
 31:       setDeviceName(device.name || device.id);
 32:       device.addEventListener("gattserverdisconnected", onDisconnect);
 33: 
 34:       const server = await device.gatt.connect();
 35:       setStatus("connected");
 36: 
 37:       const service = await server.getPrimaryService(SERVICE_UUID);
 38:       const characteristic = await service.getCharacteristic(CHAR_UUID);
 39:       charRef.current = characteristic;
 40: 
 41:       await characteristic.startNotifications();
 42:       setStatus("listening");
 43: 
 44:       let firstOffset = null; // sync offset between ESP32 and browser
 45: 
 46:       characteristic.addEventListener("characteristicvaluechanged", (event) => {
 47:         const dv = event.target.value;
 48:         let s = "";
 49:         for (let i = 0; i < dv.byteLength; i++)
 50:           s += String.fromCharCode(dv.getUint8(i));
 51: 
 52:         try {
 53:           const obj = JSON.parse(s);
 54:           const receivedAt = Date.now();
 55:           const ts_unix = Number(obj.ts_unix);
 56: 
 57:           // On first message ‚Üí sync the offset between clocks
 58:           if (firstOffset === null) {
 59:             firstOffset = ts_unix - receivedAt;
 60:             console.log("üïì Clock offset synced:", firstOffset, "ms");
 61:           }
 62: 
 63:           // Correct browser time to ESP32's NTP reference
 64:           const correctedNow = receivedAt + firstOffset;
 65: 
 66:           // Calculate true BLE latency
 67:           const latency = correctedNow - ts_unix;
 68: 
 69:           const msg = {
 70:             soil: obj.soil_percent,
 71:             light: obj.light_lx,
 72:             ts_unix,
 73:             receivedAt,
 74:             latency,
 75:             raw: s,
 76:           };
 77: 
 78:           setMessages((m) => [msg, ...m].slice(0, 100));
 79:         } catch (err) {
 80:           console.warn("Bad JSON:", s);
 81:           setMessages((m) => [
 82:             {
 83:               soil: NaN,
 84:               light: NaN,
 85:               ts_unix: NaN,
 86:               receivedAt: Date.now(),
 87:               latency: NaN,
 88:               raw: s,
 89:             },
 90:             ...m,
 91:           ]);
 92:         }
 93:       });
 94:     } catch (e) {
 95:       console.error(e);
 96:       setError(e.message || String(e));
 97:       setStatus("error");
 98:     }
 99:   }, []);
100: 
101:   return (
102:     <div className="page">
103:       <header className="header">
104:         <div className="brand">
105:           <div className="logo-dot"></div>
106:           <div className="brand-txt">
107:             <h1>PlantUp</h1>
108:             <span>BLE Pairing Demo</span>
109:           </div>
110:         </div>
111:         <div className={`status pill ${status}`}>
112:           <span className="dot"></span> {status}
113:         </div>
114:       </header>
115: 
116:       <main className="main">
117:         <ConnectionPanel
118:           status={status}
119:           deviceName={deviceName}
120:           error={error}
121:           onConnect={connectAndListen}
122:         />
123:         <MessagePanel messages={messages} />
124:       </main>
125: 
126:       <footer className="footer">
127:         <span>This page only shows the pairing process and received data.</span>
128:       </footer>
129:     </div>
130:   );
131: }
</file>

