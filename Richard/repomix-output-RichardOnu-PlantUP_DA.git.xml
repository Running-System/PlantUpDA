306:           </Text>
307:           <Text style={styles.questDescription} numberOfLines={2}>
308:             {safeDescription}
309:           </Text>
310:         </View>
311: 
312:         {canClaim ? (
313:           <TouchableOpacity onPress={onClaim} disabled={isClaimed}>
314:             <View style={styles.claimButtonStyled}>
315:               <LinearGradient
316:                 colors={['#329D9C', '#00D988']}
317:                 start={{ x: 0, y: 0 }}
318:                 end={{ x: 1, y: 0 }}
319:                 style={StyleSheet.absoluteFillObject}
320:               />
321:               <Text style={styles.claimButtonText}>CLAIM</Text>
322:             </View>
323:           </TouchableOpacity>
324:         ) : (
325:           !isClaimed && (
326:             <View style={styles.xpContainer}>
327:               <FontAwesome6
328:                 name="star"
329:                 size={12}
330:                 color="#329D9C"
331:                 style={{ marginRight: 4 }}
332:                 solid
333:               />
334:               <Text style={styles.xpText}>{quest.Quests.xp_reward} XP</Text>
335:             </View>
336:           )
337:         )}
338:         {isClaimed && <FontAwesome6 name="check" size={20} color="#329D9C" />}
339:       </View>
340: 
341:       <View style={{ marginTop: 5 }}>
342:         <View style={styles.progressBarBg}>
343:           <LinearGradient
344:             colors={['#329D9C', '#8bdcbd']}
345:             start={{ x: 0, y: 0 }}
346:             end={{ x: 1, y: 0 }}
347:             style={[styles.progressBarFill, { width: `${percentage}%` }]}
348:           />
349:         </View>
350:         <View style={{ flexDirection: 'row', justifyContent: 'space-between', marginTop: 4 }}>
351:           <Text style={styles.progressTextSubtle}>{percentage.toFixed(0)}%</Text>
352:           <Text style={styles.progressTextSubtle}>
353:             {progress}/{target}
354:           </Text>
355:         </View>
356:       </View>
357:     </View>
358:   );
