  1: #include <Arduino.h>
  2: #include <WiFi.h>
  3: #include <PubSubClient.h>
  4: #include <time.h>
  5: #include <Wire.h>
  6: #include <BH1750.h>
  7: 
  8: // ===== CONFIG =====
  9: const char *ssid = "Abudi";
 10: const char *password = "asimasim2";
 11: const char *mqtt_server = "192.168.0.244"; // your broker IP
 12: const int mqtt_port = 1883;
 13: const char *mqtt_topic = "plantup/env/data";
 14: const char *mqtt_client_id = "PlantUp_Env_ESP32";
 15: 
 16: // ===== PINS =====
 17: #define SOIL_ANALOG_PIN 1
 18: #define SOIL_DIGITAL_PIN 2
 19: #define I2C_SDA_PIN 8
 20: #define I2C_SCL_PIN 9
 21: 
 22: // ===== OBJECTS =====
 23: WiFiClient espClient;
 24: PubSubClient client(espClient);
 25: BH1750 lightMeter;
 26: 
 27: // ===== NTP =====
 28: const char *ntpServer = "pool.ntp.org";
 29: const long gmtOffset_sec = 0;
 30: const int daylightOffset_sec = 0;
 31: 
 32: // ----------------------------------------------------
 33: // 1. CONNECT WIFI + MQTT + NTP
 34: // ----------------------------------------------------
 35: void connectWiFi()
 36: {
 37:   Serial.printf("[WiFi] Connecting to %s", ssid);
 38:   WiFi.begin(ssid, password);
 39:   while (WiFi.status() != WL_CONNECTED)
 40:   {
 41:     delay(500);
 42:     Serial.print(".");
 43:   }
 44:   Serial.println("\n‚úÖ WiFi connected!");
 45:   Serial.print("[WiFi] IP: ");
 46:   Serial.println(WiFi.localIP());
 47: }
 48: 
 49: void connectMQTT()
 50: {
 51:   while (!client.connected())
 52:   {
 53:     Serial.print("[MQTT] Connecting...");
 54:     if (client.connect(mqtt_client_id))
 55:     {
 56:       Serial.println("‚úÖ Connected!");
 57:     }
 58:     else
 59:     {
 60:       Serial.printf("‚ùå Failed rc=%d. Retry in 2s\n", client.state());
 61:       delay(2000);
 62:     }
 63:   }
 64: }
 65: 
 66: void syncTime()
 67: {
 68:   configTime(gmtOffset_sec, daylightOffset_sec, ntpServer);
 69:   struct tm timeinfo;
 70:   while (!getLocalTime(&timeinfo))
 71:   {
 72:     Serial.print(".");
 73:     delay(500);
 74:   }
 75:   Serial.println("\n[NTP] Time synchronized!");
 76: }
 77: 
 78: // ----------------------------------------------------
 79: // 2. SENSOR INITIALIZATION
 80: // ----------------------------------------------------
 81: void initSensors()
 82: {
 83:   Wire.begin(I2C_SDA_PIN, I2C_SCL_PIN);
 84: 
 85:   if (lightMeter.begin(BH1750::CONTINUOUS_HIGH_RES_MODE))
 86:     Serial.println("[BH1750] ‚úÖ Ready");
 87:   else
 88:     Serial.println("[BH1750] ‚ùå Not detected!");
 89: 
 90:   pinMode(SOIL_ANALOG_PIN, INPUT);
 91:   pinMode(SOIL_DIGITAL_PIN, INPUT);
 92: }
 93: 
 94: // ----------------------------------------------------
 95: // 3. PUBLISH SENSOR DATA (same as sendFixedData BLE)
 96: // ----------------------------------------------------
 97: void publishData()
 98: {
 99:   if (!client.connected())
100:     connectMQTT();
101: 
102:   // --- Read BH1750 ---
103:   float lightLux = 0.0f;
104:   if (lightMeter.measurementReady(true))
105:     lightLux = lightMeter.readLightLevel();
106: 
107:   // --- Read soil sensor (LM393) ---
108:   int soilAnalog = analogRead(SOIL_ANALOG_PIN);
109:   int soilDigital = digitalRead(SOIL_DIGITAL_PIN);
110:   float soilPercent = 100.0f - ((soilAnalog / 4095.0f) * 100.0f);
111: 
112:   // --- Timestamp ---
113:   time_t now;
114:   time(&now);
115:   unsigned long long ts_ms = (unsigned long long)now * 1000ULL;
116: 
117:   // --- JSON Payload ---
118:   char payload[200];
119:   snprintf(payload, sizeof(payload),
120:            "{\"soil_percent\":%.1f,"
121:            "\"soil_raw\":%d,"
122:            "\"soil_digital\":%d,"
123:            "\"light_lx\":%.1f,"
124:            "\"ts_unix\":%llu}",
125:            soilPercent,
126:            soilAnalog,
127:            soilDigital,
128:            lightLux,
129:            ts_ms);
130: 
131:   // --- Publish ---
132:   client.publish(mqtt_topic, payload);
133:   Serial.print("[MQTT] ‚Üí ");
134:   Serial.println(payload);
135: }
136: 
137: // ----------------------------------------------------
138: // 4. MAIN
139: // ----------------------------------------------------
140: void setup()
141: {
142:   Serial.begin(115200);
143:   delay(500);
144:   Serial.println("\n[BOOT] PlantUp MQTT Publisher (Real Sensors)");
145: 
146:   connectWiFi();
147:   syncTime();
148:   initSensors();
149: 
150:   client.setServer(mqtt_server, mqtt_port);
151:   connectMQTT();
152: }
153: 
154: void loop()
155: {
156:   static unsigned long lastSend = 0;
157:   unsigned long now = millis();
158: 
159:   if (now - lastSend > 1000)
160:   {
161:     lastSend = now;
162:     publishData();
163:   }
164: 
165:   client.loop();
166: }
</file>

