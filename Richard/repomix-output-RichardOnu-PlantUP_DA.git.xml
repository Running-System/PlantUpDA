<file path="PlantUp/src/utils/leveling.ts">
  1: import { ImageSourcePropType } from 'react-native';
  2: 
  3: // --- TYPES ---
  4: export type Reward = {
  5:   level: number;
  6:   title: string;
  7:   image?: ImageSourcePropType; // Placeholder for now
  8:   description: string;
  9: };
 10: 
 11: // --- CONSTANTS ---
 12: export const MAX_LEVEL = 100;
 13: 
 14: export const REWARDS: Reward[] = [
 15:   { level: 5, title: 'New Plant Slot', description: 'Unlock a new slot for your garden.' },
 16:   { level: 10, title: 'Sprout Badge', description: 'Show off your progress with a new badge.' },
 17:   { level: 15, title: 'Fertilizer Pack', description: 'Boost your plants growth.' },
 18:   { level: 20, title: 'Gardener Title', description: 'You are now a recognized Gardener.' },
 19:   { level: 30, title: 'Rare Seed', description: 'A mysterious seed for your collection.' },
 20:   { level: 40, title: 'Watering Can Skin', description: 'Style your tools.' },
 21:   { level: 50, title: 'Green Thumb Badge', description: 'Halfway to mastery!' },
 22:   { level: 60, title: 'Greenhouse BG', description: 'A new background for your profile.' },
 23:   { level: 75, title: 'Master Title', description: 'Respect from the community.' },
 24:   { level: 100, title: 'Legendary Trophy', description: 'The ultimate gardening achievement.' },
 25: ];
 26: 
 27: // --- FUNCTIONS ---
 28: 
 29: /**
 30:  * Calculates the XP required to move from current level to the next.
 31:  * System 2 Formulas:
 32:  * Levels 1‚Äì10: XP_next(L) = round(99.9141 * L^1.2)
 33:  * Levels 11‚Äì50: XP_next(L) = round(1151.6746 + 5.765454 * L^1.8)
 34:  * Levels 51‚Äì99: XP_next(L) = round(4500 + 5.017007 * (L - 51))
 35:  * Level 100: 0
 36:  */
 37: export const xpToNextLevel = (level: number): number => {
 38:   if (level >= MAX_LEVEL) return 0;
 39: 
 40:   let xp = 0;
 41:   if (level >= 1 && level <= 10) {
 42:     xp = 99.9141 * Math.pow(level, 1.2);
 43:   } else if (level >= 11 && level <= 50) {
 44:     xp = 1151.6746 + 5.765454 * Math.pow(level, 1.8);
 45:   } else if (level >= 51 && level < 100) {
 46:     xp = 4500 + 5.017007 * (level - 51);
 47:   }
 48: 
 49:   return Math.round(xp);
 50: };
 51: 
 52: /**
 53:  * Calculates the total cumulative XP required to reach a specific level.
 54:  * This replaces the static XP_TABLE.
 55:  */
 56: export const getTotalXPForLevel = (level: number): number => {
 57:   if (level <= 1) return 0;
 58: 
 59:   let total = 0;
 60:   for (let i = 1; i < level; i++) {
 61:     total += xpToNextLevel(i);
 62:   }
 63:   return total;
 64: };
 65: 
 66: export const getLevelFromXP = (xp: number): number => {
 67:   // Since we don't have a lookup table, we iterate up to MAX_LEVEL
 68:   // Optimization: We could approximate or use binary search if MAX_LEVEL was huge,
 69:   // but for 100 levels, a simple loop is fast enough.
 70:   for (let i = 1; i < MAX_LEVEL; i++) {
 71:     const xpForNext = getTotalXPForLevel(i + 1);
 72:     if (xp < xpForNext) {
 73:       return i;
 74:     }
 75:   }
 76:   return MAX_LEVEL;
 77: };
 78: 
 79: export const getXPForNextLevel = (currentLevel: number): number => {
 80:   if (currentLevel >= MAX_LEVEL) return getTotalXPForLevel(MAX_LEVEL);
 81:   return getTotalXPForLevel(currentLevel + 1);
 82: };
 83: 
 84: export const getLevelProgress = (xp: number, level: number) => {
 85:   if (level >= MAX_LEVEL) return 1;
 86: 
 87:   const startXP = getTotalXPForLevel(level);
 88:   const nextXP = getTotalXPForLevel(level + 1);
 89: 
 90:   const needed = nextXP - startXP;
 91:   const current = xp - startXP;
 92: 
 93:   if (needed <= 0) return 1; // Should not happen for levels < 100
 94: 
 95:   return Math.min(Math.max(current / needed, 0), 1);
 96: };
 97: 
 98: export const getNextReward = (level: number): Reward | null => {
 99:   return REWARDS.find(r => r.level > level) || null;
100: };
101: 
102: export const RANKS = [
103:   { name: 'Seedling', minLevel: 1, maxLevel: 16 },
104:   { name: 'Sprout', minLevel: 17, maxLevel: 33 },
105:   { name: 'Sapling', minLevel: 34, maxLevel: 50 },
106:   { name: 'Evergreen', minLevel: 51, maxLevel: 67 },
107:   { name: 'Sunflower', minLevel: 68, maxLevel: 83 },
108:   { name: 'Roseheart', minLevel: 84, maxLevel: 100 },
109: ];
110: 
111: export const getRankName = (level: number): string => {
112:   const rank = RANKS.find(r => level >= r.minLevel && level <= r.maxLevel);
113:   return rank ? rank.name : 'Seedling';
114: };
</file>

