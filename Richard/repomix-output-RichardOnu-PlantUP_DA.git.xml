<file path="ESP32/Prototypes/mqtt-client/src/App.jsx">
  1: import React, { useEffect, useState } from "react";
  2: import mqtt from "mqtt";
  3: import ConnectionPanel from "./components/ConnectionPanel";
  4: import MessagePanel from "./components/MessagePanel";
  5: 
  6: const MQTT_URL = "ws://192.168.0.244:9001"; // your broker websocket port (default: 9001)
  7: const TOPIC = "plantup/env/data";
  8: 
  9: export default function App() {
 10:   const [status, setStatus] = useState("idle");
 11:   const [messages, setMessages] = useState([]);
 12:   const [error, setError] = useState("");
 13:   const [client, setClient] = useState(null);
 14: 
 15:   /* Auto Connecting there is no connection processs MQTT
 16: useEffect(() => {
 17:   connectAndSubscribe();
 18: }, []); */
 19: 
 20:   const connectAndSubscribe = () => {
 21:     try {
 22:       setStatus("connecting");
 23:       const c = mqtt.connect(MQTT_URL);
 24: 
 25:       c.on("connect", () => {
 26:         setStatus("connected");
 27:         c.subscribe(TOPIC);
 28:         console.log("‚úÖ Subscribed to", TOPIC);
 29:       });
 30: 
 31:       c.on("message", (topic, msgBuffer) => {
 32:         try {
 33:           const msg = JSON.parse(msgBuffer.toString());
 34:           const receivedAt = Date.now();
 35:           const ts_unix = Number(msg.ts_unix);
 36: 
 37:           // Maintain a static offset between ESP and browser
 38:           if (window.firstOffset === undefined) {
 39:             window.firstOffset = ts_unix - receivedAt;
 40:             console.log("üïì Clock offset synced:", window.firstOffset, "ms");
 41:           }
 42: 
 43:           const correctedNow = receivedAt + window.firstOffset;
 44:           const latency = correctedNow - ts_unix;
 45: 
 46:           setMessages((prev) => [
 47:             {
 48:               soil_percent: Number(msg.soil_percent),
 49:               light_lx: Number(msg.light_lx),
 50:               receivedAt,
 51:               latency,
 52:               raw: msgBuffer.toString(),
 53:             },
 54:             ...prev,
 55:           ]);
 56:         } catch (err) {
 57:           console.warn("Bad message:", msgBuffer.toString());
 58:         }
 59:       });
 60: 
 61:       c.on("error", (err) => {
 62:         setError(err.message);
 63:         setStatus("error");
 64:       });
 65: 
 66:       setClient(c);
 67:     } catch (e) {
 68:       setError(e.message);
 69:       setStatus("error");
 70:     }
 71:   };
 72: 
 73:   return (
 74:     <div className="page">
 75:       <header className="header">
 76:         <div className="brand">
 77:           <div className="logo-dot"></div>
 78:           <div className="brand-txt">
 79:             <h1>PlantUp</h1>
 80:             <span>MQTT Demo</span>
 81:           </div>
 82:         </div>
 83:         <div className={`status pill ${status}`}>
 84:           <span className="dot"></span> {status}
 85:         </div>
 86:       </header>
 87: 
 88:       <main className="main">
 89:         <ConnectionPanel
 90:           status={status}
 91:           error={error}
 92:           onConnect={connectAndSubscribe}
 93:           deviceName={"MQTT Broker"}
 94:         />
 95:         <MessagePanel messages={messages} />
 96:       </main>
 97: 
 98:       <footer className="footer">
 99:         <span>
100:           This page subscribes to MQTT messages from your PlantUp device.
101:         </span>
102:       </footer>
103:     </div>
104:   );
105: }
