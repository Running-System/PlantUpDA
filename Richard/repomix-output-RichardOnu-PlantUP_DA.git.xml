108: export default function ShowPlantStats() {
109:   const navigation = useNavigation<any>();
110:   const route = useRoute<any>();
111:   const { theme } = useUnistyles();
112: 
113:   const { plantId: initialPlantId } = route.params ?? {};
114: 
115:   // State
116:   const [userId, setUserId] = useState<string | null>(null);
117:   const [name, setName] = useState('--');
118:   const [temperature, setTemperature] = useState('‚Äî'); // Weather
119:   const [humidity, setHumidity] = useState('‚Äî'); // Weather
120:   const [selectedPlant, setSelectedPlant] = useState<any>(null);
121: 
122:   // Data State
123:   const [latest, setLatest] = useState<PlantController | null>(null);
124:   const [historyData, setHistoryData] = useState<PlantController[]>([]);
125: 
126:   // UI State
127:   const [loading, setLoading] = useState(true);
128:   const [loadingHistory, setLoadingHistory] = useState(false); // New state for background loading
129:   const [isDropdownVisible, setDropdownVisible] = useState(false);
130:   const [selectedOptionLabel, setSelectedOptionLabel] = useState<
131:     'Temperature' | 'Humidity' | 'Soil Moisture' | 'Light' | 'EC'
132:   >('Temperature');
133:   const [timeRange, setTimeRange] = useState<TimeRangeKey>('Last 1 Hour (5 Recs)');
134:   const [barColor, setBarColor] = useState('#fdae11');
135: 
136:   const isDataFreshRef = useRef(false);
137: 
138:   // üü¢ 1. MEMOIZATION: Calculate Chart & MiniTrend data only when history changes
139:   // This prevents iterating over 1000 items 5 times on every render
140:   const { chartData, maxY, sensorTrends } = useMemo(() => {
141:     if (!historyData.length) {
142:       return { chartData: [], maxY: 0, sensorTrends: { soil: [], temp: [], light: [], ec: [] } };
143:     }
144: 
145:     // Prepare MiniTrend Data (Single iteration)
146:     const sensorTrends = {
147:       soil: [] as (number | null)[],
148:       temp: [] as (number | null)[],
149:       light: [] as (number | null)[],
150:       ec: [] as (number | null)[],
151:     };
152: 
153:     historyData.forEach((p) => {
154:       sensorTrends.soil.push(p.soil_moisture);
155:       sensorTrends.temp.push(p.temperature);
156:       sensorTrends.light.push(p.light);
157:       sensorTrends.ec.push(p.electrical_conductivity);
158:     });
159: 
160:     // Prepare Chart Data
161:     // Note: Reversing array can be expensive, do it only if necessary for the chart library
162:     const sortedHistory = [...historyData].reverse();
163: 
164:     const cData = sortedHistory.map((p) => {
165:       const label = new Date(p.created_at).toISOString().slice(11, 16);
166:       let value = 0;
167:       switch (selectedOptionLabel) {
168:         case 'Temperature':
169:           value = p.temperature ?? 0;
170:           break;
171:         case 'Humidity':
172:           value = p.humidity ?? 0;
173:           break;
174:         case 'Soil Moisture':
175:           value = p.soil_moisture ?? 0;
176:           break;
177:         case 'Light':
178:           value = p.light ?? 0;
179:           break;
180:         case 'EC':
181:           value = p.electrical_conductivity ?? 0;
182:           break;
183:       }
184:       return { value, label, frontColor: barColor };
185:     });
186: 
187:     const mY = cData.length ? Math.max(...cData.map((d) => d.value)) : 0;
188: 
189:     return { chartData: cData, maxY: mY, sensorTrends };
190:   }, [historyData, selectedOptionLabel, barColor]);
191: 
192:   // üü¢ 2. QUEST CHECKER: Fire and forget (doesn't block UI)
193:   const runQuestCheck = async (uid: string) => {
194:     try {
195:       const { data: quests } = await db.gamification
196:         .from('User_quests')
197:         .select(`id, progress, status, quest:Quests (action_code, target_count)`)
198:         .eq('user_id', uid)
199:         .eq('status', 'active'); // ‚úÖ only active quests
200: 
201:       if (!quests) return;
202: 
203:       const updates = quests
204:         .filter((q: any) => {
205:           const target = q.quest?.target_count ?? 0;
206:           const current = q.progress ?? 0;
207: 
208:           return (
209:             q.quest?.action_code === 'CHECK_STATUS' && current < target // ‚úÖ only update if not already reached
210:           );
211:         })
212:         .map((uq: any) => {
213:           const newProgress = (uq.progress ?? 0) + 1;
214:           const target = uq.quest?.target_count ?? 0;
215: 
216:           return db.gamification
217:             .from('User_quests')
218:             .update({
219:               progress: newProgress,
220:               status: newProgress >= target ? 'completed' : 'active',
221:               completed_at: newProgress >= target ? new Date().toISOString() : null,
222:             })
223:             .eq('id', uq.id);
224:         });
225: 
226:       await Promise.all(updates);
227:     } catch (err) {
228:       console.error('runQuestCheck error:', err);
229:     }
230:   };
231: 
232:   // üü¢ 3. WEATHER FETCHER: Separated from critical path
233:   const fetchWeather = async () => {
234:     try {
235:       const { status } = await Location.requestForegroundPermissionsAsync();
236:       if (status !== 'granted') return;
237: 
238:       const loc = await Location.getCurrentPositionAsync({ accuracy: Location.Accuracy.Balanced }); // Balanced is faster
239:       const url = `https://api.open-meteo.com/v1/forecast?latitude=${loc.coords.latitude}&longitude=${loc.coords.longitude}&current_weather=true&hourly=relative_humidity_2m`;
240: 
241:       const res = await fetch(url);
242:       const data = await res.json();
243: 
244:       if (data?.current_weather?.temperature != null)
245:         setTemperature(`${data.current_weather.temperature.toFixed(1)}¬∞`);
246:       if (data?.hourly?.relative_humidity_2m?.[0] != null)
247:         setHumidity(`${data.hourly.relative_humidity_2m[0]}%`);
248:     } catch (e) {
249:       console.log('Weather fetch silent fail');
250:     }
251:   };
252: 
253:   // üü¢ 4. CORE DATA LOGIC
254:   const fetchPlantData = useCallback(async (plantId: string, range: TimeRangeKey) => {
255:     setLoadingHistory(true);
256: 
257:     try {
258:       // Step A: Load Cache immediately to show something
259:       const cacheKey = CACHE_KEY_PREFIX + plantId;
260:       const cached = await getCache(cacheKey);
261: 
262:       let latestTimestamp = null;
263: 
264:       if (cached && cached.data) {
265:         setHistoryData(cached.data);
266:         // If we don't have a "Latest" yet, use the cache's first item
267:         setLatest((prev) => prev || cached.data[0]);
268:         latestTimestamp = cached.timestamp;
269:       }
270: 
271:       // Step B: Get current server latest (Fast - 1 row)
272:       const serverLatest = await fetchLatestController(plantId);
273: 
274:       if (serverLatest) {
275:         setLatest(serverLatest);
276: 
277:         // Determine if we need to fetch full history
278:         // 1. If no cache
279:         // 2. If server data is newer than cache
280:         // 3. If user changed time range (we might need more data than what is cached)
281:         const isServerNewer =
282:           !latestTimestamp || new Date(serverLatest.created_at) > new Date(latestTimestamp);
283: 
284:         if (isServerNewer || range !== 'Last 1 Hour (5 Recs)') {
285:           const limit = TIME_RANGES[range];
286:           const { data: newData } = await db.micro
287:             .from('Controllers')
288:             .select(
289:               'id, temperature, humidity, soil_moisture, light, electrical_conductivity, created_at'
290:             )
291:             .eq('plant_id', plantId)
292:             .order('created_at', { ascending: false })
293:             .limit(limit);
294: 
295:           if (newData && newData.length > 0) {
296:             setHistoryData(newData as PlantController[]);
297:             isDataFreshRef.current = true;
298: 
299:             // Only cache large datasets to save IO operations
300:             if (limit >= 50) {
301:               await setCache(cacheKey, { timestamp: serverLatest.created_at, data: newData });
302:             }
303:           }
304:         } else {
305:           isDataFreshRef.current = false;
306:         }
307:       }
308:     } catch (err) {
309:       console.error('Data fetch error:', err);
310:     } finally {
311:       setLoading(false);
312:       setLoadingHistory(false);
313:     }
314:   }, []);
315: 
316:   // üü¢ 5. INITIAL MOUNT (Parallel Execution)
317:   const initializeScreen = useCallback(async () => {
318:     setLoading(true);
319:     try {
320:       // 1. Get Auth and Plants first
321:       const { data: auth } = await db.public.auth.getUser();
322:       const user = auth?.user;
323:       if (!user) throw new Error('No user');
324:       setUserId(user.id);
325: 
326:       const { data: plants } = await db.social
327:         .from('Plants')
328:         .select('id, nickname')
329:         .eq('user_id', user.id);
330: 
331:       const targetPlant = plants?.find((p) => p.id === initialPlantId) || plants?.[0];
332: 
333:       if (targetPlant) {
334:         setSelectedPlant(targetPlant);
335:         setName(targetPlant.nickname);
336: 
337:         // 2. PARALLEL EXECUTION: Fire all 3 tasks at once
338:         await Promise.allSettled([
339:           fetchPlantData(targetPlant.id, timeRange), // Main Data
340:           fetchWeather(), // External API (don't await strictly)
341:           runQuestCheck(user.id), // Background Task
342:         ]);
343:       } else {
344:         setLoading(false);
345:       }
346:     } catch (e) {
347:       console.error(e);
348:       setLoading(false);
349:     }
350:   }, [initialPlantId, fetchPlantData]); // Removed timeRange dependency to prevent loop
351: 
352:   useFocusEffect(
353:     useCallback(() => {
354:       initializeScreen();
355:     }, [initializeScreen])
356:   );
357: 
358:   // Handle Time Range Change specifically
359:   useEffect(() => {
360:     if (selectedPlant?.id && !loading) {
361:       fetchPlantData(selectedPlant.id, timeRange);
362:     }
363:   }, [timeRange]);
364: 
365:   const health = latest ? calculateHealth(latest) : null;
366: 
367:   if (loading) return <LoadingScreen />;
368: 
369:   return (
370:     <GestureHandlerRootView style={{ flex: 1 }}>
371:       <View style={styles.container}>
372:         {/* Header Background */}
373:         <View style={styles.headerBg}>
374:           <Image
375:             source={require('../../../../assets/images/statsBackground.png')}
376:             style={styles.headerImage}
377:             resizeMode="cover"
378:           />
379:           <View style={styles.headerContent}>
