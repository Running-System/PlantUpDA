<file path="PlantUp/src/features/plantstats/api/healthScore.ts">
 1: export function calculateHealth(r: any): number {
 2:   if (!r) return 0;
 3: 
 4:   const ranges = {
 5:     temperature: { min: 18, max: 30 },
 6:     humidity: { min: 40, max: 70 },
 7:     soil_moisture: { min: 30, max: 70 },
 8:     light: { min: 500, max: 20000 },
 9:     electrical_conductivity: { min: 0.5, max: 3 },
10:   };
11: 
12:   let score = 100;
13:   const apply = (v: any, key: keyof typeof ranges, weight = 15) => {
14:     if (v == null) return;
15:     if (v < ranges[key].min || v > ranges[key].max) score -= weight;
16:   };
17: 
18:   apply(r.temperature, 'temperature');
19:   apply(r.humidity, 'humidity');
20:   apply(r.soil_moisture, 'soil_moisture');
21:   apply(r.light, 'light');
22:   apply(r.electrical_conductivity, 'electrical_conductivity', 20);
23: 
24:   return Math.max(0, Math.min(score, 100));
25: }
</file>

<file path="PlantUp/src/features/plantstats/api/history.ts">
 1: export function calculateAverages(data: any[]) {
 2:   if (!data.length) return null;
 3: 
 4:   const avg = (arr: number[]) => (arr.length ? arr.reduce((a, b) => a + b, 0) / arr.length : 0);
 5: 
 6:   return {
 7:     temperature: avg(data.map((d) => d.temperature ?? 0)),
 8:     soil_moisture: avg(data.map((d) => d.soil_moisture ?? 0)),
 9:     ec: avg(data.map((d) => d.electrical_conductivity ?? 0)),
10:     light: avg(data.map((d) => d.light ?? 0)),
11:   };
12: }
13: 
14: export function lastXdays(controllerRows: any[], days: number) {
15:   const cutoff = Date.now() - days * 24 * 60 * 60 * 1000;
16:   return controllerRows.filter((p) => new Date(p.created_at).getTime() >= cutoff);
17: }
