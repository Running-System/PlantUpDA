43: bool WiFiManager::connectToWiFi()
44: {
45:     if (ssid.isEmpty())
46:         return false;
47: 
48:     Serial.printf("[WiFi] Connecting to %s...\n", ssid.c_str());
49:     WiFi.mode(WIFI_STA);
50:     WiFi.begin(ssid.c_str(), password.c_str());
51: 
52:     unsigned long start = millis();
53:     while (WiFi.status() != WL_CONNECTED && millis() - start < 10000)
54:     {
55:         delay(500);
56:         Serial.print(".");
57:     }
58:     Serial.println();
59: 
60:     return WiFi.status() == WL_CONNECTED;
61: }
