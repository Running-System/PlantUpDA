 1: using System.Diagnostics;
 2: 
 3: namespace VideoAPI.Services;
 4: 
 5: public class VideoProcessor
 6: {
 7:     private readonly string _ffmpegPath;
 8: 
 9:     public VideoProcessor()
10:     {
11:         _ffmpegPath = Environment.GetEnvironmentVariable("FFMPEG_PATH") ?? "ffmpeg";
12:     }
13: 
14:     /// Komprimiert MP4 auf H.264/AAC, max 720p, CRF 28; gibt Pfad zur neuen Tempdatei zur√ºck.
15:     public async Task<string> CompressAsync(string inputPath)
16:     {
17:         Console.WriteLine($"FFmpeg compression started for: {inputPath}");
18: 
19:         var outputPath = Path.ChangeExtension(Path.GetTempFileName(), ".mp4");
20:         Console.WriteLine($"Output path: {outputPath}");
21: 
22:         var args =
23:         $"-i \"{inputPath}\" " +
24:         "-c:v libx264 -crf 28 -preset veryfast " +
25:         "-vf \"scale='min(1920,iw)':'min(1080,ih)':force_original_aspect_ratio=decrease:force_divisible_by=2\" " +
26:         "-c:a aac -b:a 96k -movflags +faststart " +
27:         $"\"{outputPath}\" -y";
28: 
29:         Console.WriteLine($"FFmpeg command: {_ffmpegPath} {args}");
30: 
31:         var psi = new ProcessStartInfo(_ffmpegPath, args)
32:         {
33:             RedirectStandardError = true,
34:             UseShellExecute = false,
35:             CreateNoWindow = true
36:         };
37: 
38:         using var proc = Process.Start(psi)!;
39:         var err = await proc.StandardError.ReadToEndAsync();
40:         await proc.WaitForExitAsync();
41: 
42:         Console.WriteLine($"FFmpeg exit code: {proc.ExitCode}");
43: 
44:         if (proc.ExitCode != 0)
45:         {
46:             Console.WriteLine($"FFmpeg error: {err}");
47:             throw new Exception("FFmpeg failed: " + err);
48:         }
49: 
50:         Console.WriteLine("FFmpeg compression successful");
51:         return outputPath;
52:     }
53: }
