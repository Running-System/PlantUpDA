 22: 
 23: // --- IMPORTE F√úR UPLOAD ---
 24: import * as ImagePicker from 'expo-image-picker';
 25: import * as DocumentPicker from 'expo-document-picker';
 26: import * as FileSystem from 'expo-file-system';
 27: 
 28: import { db } from '../../../utils/supabase';
 29: 
 30: const { height } = Dimensions.get('window');
 31: 
 32: // Limit: 50 MB (Damit auch Videos passen)
 33: const MAX_SIZE = 50 * 1024 * 1024;
 34: 
 35: export default function CreatePostScreen() {
 36:   const { theme } = useUnistyles();
 37:   const navigation = useNavigation();
 38:   const scrollViewRef = useRef(null);
 39: 
 40:   // --- STATE ---
 41:   const [caption, setCaption] = useState('');
 42:   const [username, setUsername] = useState('Loading...');
 43:   const [userId, setUserId] = useState<string | null>(null);
 44:   const [loading, setLoading] = useState(false);
 45: 
 46:   // Modal State
 47:   const [showAttachmentMenu, setShowAttachmentMenu] = useState(false);
 48: 
 49:   // Media State
 50:   const [media, setMedia] = useState<{
 51:     uri: string;
 52:     type: 'image' | 'video' | 'document' | 'sensor';
 53:     mimeType?: string;
 54:     name?: string;
 55:     sensorData?: any;
 56:   } | null>(null);
 57: 
 58:   const profileImage = require('../../../../assets/images/Profilbild.png');
 59: 
 60:   // 1. USER LADEN
 61:   useEffect(() => {
 62:     const fetchUser = async () => {
 63:       const {
 64:         data: { user },
 65:       } = await db.public.auth.getUser();
 66:       if (user) {
 67:         setUserId(user.id);
 68:         const { data } = await db.user.from('Users').select('username').eq('id', user.id).single();
 69:         if (data) setUsername(data.username);
 70:       }
 71:     };
 72:     fetchUser();
 73:   }, []);
 74: 
 75:   const closeMenu = () => setShowAttachmentMenu(false);
 76: 
 77:   // --- FUNKTION: FOTO & VIDEO AUSW√ÑHLEN ---
 78:   const pickMedia = async () => {
 79:     closeMenu();
 80:     const res = await ImagePicker.launchImageLibraryAsync({
 81:       mediaTypes: ImagePicker.MediaTypeOptions.All,
 82:       quality: 0.7,
 83:       videoMaxDuration: 60,
 84:       allowsEditing: true,
 85:     });
 86: 
 87:     if (!res.canceled) {
 88:       const asset = res.assets[0];
 89: 
 90:       if (asset.fileSize && asset.fileSize > MAX_SIZE) {
 91:         Alert.alert('Too big', 'File is over 50MB.');
 92:         return;
 93:       }
 94: 
 95:       setMedia({
 96:         uri: asset.uri,
 97:         type: asset.type === 'video' ? 'video' : 'image',
 98:         mimeType: asset.type === 'video' ? 'video/mp4' : 'image/jpeg',
 99:         name: asset.fileName || `upload.${asset.type === 'video' ? 'mp4' : 'jpg'}`,
100:       });
101:     }
102:   };
103: 
104:   // --- FUNKTION: DOKUMENT AUSW√ÑHLEN ---
105:   const pickDocument = async () => {
106:     closeMenu();
107:     try {
108:       const res = await DocumentPicker.getDocumentAsync({
109:         type: '*/*',
110:         copyToCacheDirectory: true,
111:       });
112: 
113:       if (!res.canceled) {
114:         const asset = res.assets[0];
115: 
116:         if (asset.size && asset.size > MAX_SIZE) {
117:           Alert.alert('Too big', 'Max 50MB.');
118:           return;
119:         }
120: 
121:         setMedia({
122:           uri: asset.uri,
123:           type: 'document',
124:           name: asset.name,
125:           mimeType: asset.mimeType || 'application/octet-stream',
126:         });
127:       }
128:     } catch (e) {
129:       console.log(e);
130:     }
131:   };
132: 
133:   // --- FUNKTION: SENSOR DATEN ---
134:   const attachSensorData = async () => {
135:     closeMenu();
136:     if (!userId) return;
137:     setLoading(true);
138:     try {
139:       const { data: plants } = await db.social
140:         .from('Plants')
141:         .select('id, nickname')
142:         .eq('user_id', userId)
143:         .limit(1);
144:       if (!plants?.length) {
145:         Alert.alert('No Plants', 'Add a plant first.');
146:         return;
147:       }
148: 
149:       const { data: reading } = await db.micro
150:         .from('Controllers')
151:         .select('*')
152:         .eq('plant_id', plants[0].id)
153:         .order('time', { ascending: false })
154:         .limit(1)
155:         .single();
156: 
157:       if (reading) {
158:         setMedia({
159:           uri: '',
160:           type: 'sensor',
161:           name: `${plants[0].nickname} Status`,
162:           sensorData: reading,
163:         });
164:       } else {
165:         Alert.alert('No Data', 'No sensor data found.');
166:       }
167:     } catch (e) {
168:       console.log(e);
169:     } finally {
170:       setLoading(false);
171:     }
172:   };
173: 
174:   // --- HAUPTFUNKTION: POSTEN ---
175:   const handlePost = async () => {
176:     if (!caption && !media) {
177:       Alert.alert('Empty', 'Please write something.');
178:       return;
179:     }
180: 
181:     setLoading(true);
182: 
183:     try {
184:       let finalUrl = null;
185:       let finalType = 'text';
186:       let finalCaption = caption;
187: 
188:       if (media && media.type !== 'sensor') {
189:         const {
190:           data: { session },
191:         } = await db.public.auth.getSession();
192:         if (!session) throw new Error('No active session');
193:         const token = session.access_token;
194: 
195:         const supabaseUrl = process.env.EXPO_PUBLIC_SUPABASE_URL;
196: 
197:         const ext = media.name?.split('.').pop() || 'mp4';
198:         const fileName = `${Date.now()}.${ext}`;
199:         const filePath = `${userId}/${fileName}`;
200: 
201:         const uploadUrl = `${supabaseUrl}/storage/v1/object/post-media/${filePath}`;
202: 
203:         const uploadResult = await FileSystem.uploadAsync(uploadUrl, media.uri, {
204:           httpMethod: 'POST',
205:           headers: {
206:             Authorization: `Bearer ${token}`,
207:             'Content-Type': media.mimeType || 'application/octet-stream',
208:           },
209:           uploadType: FileSystem.FileSystemUploadType.BINARY_CONTENT,
210:         });
211: 
212:         if (uploadResult.status !== 200) {
213:           console.log('Upload error:', uploadResult.body);
214:           throw new Error('Upload failed: ' + uploadResult.body);
215:         }
216: 
217:         const { data } = db.public.storage.from('post-media').getPublicUrl(filePath);
218:         finalUrl = data.publicUrl;
219:         finalType = media.type;
220:       } else if (media && media.type === 'sensor') {
221:         finalType = 'sensor';
222:         const d = media.sensorData;
223:         finalCaption += `\n\nüåø Status:\nüå°Ô∏è ${d.temperature}¬∞C | üíß ${d.soil_moisture}%`;
224:       }
225: 
226:       const { error } = await db.social.from('Posts').insert({
227:         user_id: userId,
228:         caption: finalCaption,
229:         media_url: finalUrl,
230:         type: finalType,
231:         likes: 0,
232:         shares: 0,
233:         comments_count: 0,
234:       });
235: 
236:       if (error) throw error;
237: 
238:       Alert.alert('Success', 'Post created!');
239: 
240:       // >>> RESET HERE <<<
241:       setCaption('');
242:       setMedia(null);
243:     } catch (e: any) {
244:       console.error('Post Error:', e);
245:       Alert.alert('Error', e.message || 'Network request failed');
246:     } finally {
247:       setLoading(false);
248:     }
249:   };
