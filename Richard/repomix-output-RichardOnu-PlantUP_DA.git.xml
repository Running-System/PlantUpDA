10: 
11: // === Global module instances ===
12: SDManager sdManager;
13: WiFiManager wifiManager(sdManager);
14: WebPortal webPortal;
15: Sensors sensors;
16: MQTTClientWrapper mqttClient;
17: 
18: // === Setup ===
19: void setup()
20: {
21:     Serial.begin(115200);
22:     delay(500);
23:     Serial.println("\n=== Plant Up! Controller Boot ===");
24: 
25:     // Mount filesystem (for logo and assets)
26:     if (!LittleFS.begin())
27:     {
28:         Serial.println("[LittleFS] Mount failed ‚Äî logo won‚Äôt load.");
29:     }
30:     else
31:     {
32:         Serial.println("[LittleFS] Mounted successfully.");
33:     }
34: 
35:     // Try loading credentials from SD
36:     if (!wifiManager.loadCredentials())
37:     {
38:         Serial.println("[Setup] No Wi-Fi credentials found ‚Üí starting AP setup mode.");
39:         webPortal.begin(&wifiManager); // start AsyncWebServer (AP mode)
40:         return;                        // stay in AP mode until reboot
41:     }
42: 
43:     // Connect using stored credentials
44:     if (wifiManager.connectToWiFi())
45:     {
46:         Serial.println("[Setup] Wi-Fi connected successfully!");
47:         Serial.print("[Wi-Fi] IP: ");
48:         Serial.println(WiFi.localIP());
49: 
50:         // Init sensors and MQTT
51:         sensors.begin();
52:         mqttClient.begin();
53:     }
54:     else
55:     {
56:         Serial.println("[Setup] Wi-Fi connection failed ‚Üí fallback to AP setup mode.");
57:         webPortal.begin(&wifiManager);
58:     }
59: }
60: 
61: // === Main Loop ===
62: void loop()
63: {
64:     // If Wi-Fi is connected, handle MQTT + sensor publishing
65:     if (wifiManager.isConnected())
66:     {
67:         mqttClient.loop();
68:         sensors.publishData(mqttClient);
69:         delay(5000); // publish interval
70:     }
71:     else
72:     {
73:         // AsyncWebServer runs independently ‚Äî just idle
74:         delay(200);
75:     }
76: }
