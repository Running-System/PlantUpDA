  4: import { StyleSheet, useUnistyles } from 'react-native-unistyles';
  5: 
  6: import { trendOptions } from '../api/options';
  7: 
  8: export default function HistoryCard({ plants, calculateAverages, lastXdays }: any) {
  9:   const { theme } = useUnistyles();
 10: 
 11:   if (!plants?.length) return null;
 12: 
 13:   const safeWeek = calculateAverages(lastXdays(plants, 7)) ?? {};
 14:   const safeMonth = calculateAverages(lastXdays(plants, 30)) ?? {};
 15: 
 16:   // Map trendOptions -> actual plant keys
 17:   const keyMap: any = {
 18:     temperature: 'temperature',
 19:     humidity: 'humidity',
 20:     soil: 'soil_moisture',
 21:     light: 'light',
 22:     ec: 'electrical_conductivity',
 23:   };
 24: 
 25:   // Helper: safe numeric
 26:   const toSafeNumber = (val: any, fallback = 0) => {
 27:     const n = typeof val === 'number' ? val : val != null ? Number(val) : fallback;
 28: 
 29:     return Number.isFinite(n) ? n : fallback;
 30:   };
 31: 
 32:   // Helper: safe sparkline generator
 33:   const getSparkFor = (valueKey: string) => {
 34:     const plantKey = keyMap[valueKey];
 35: 
 36:     const list = lastXdays(plants, 30).map((p: any) => ({
 37:       value: toSafeNumber(p?.[plantKey], 0),
 38:     }));
 39: 
 40:     // gifted-charts dies on empty / NaN arrays
 41:     const cleaned = list.filter((d) => Number.isFinite(d.value));
 42: 
 43:     return cleaned.length ? cleaned : [{ value: 0 }];
 44:   };
 45: 
 46:   return (
 47:     <View style={[styles.card, { backgroundColor: theme.colors.background }]}>
 48:       <Text style={[styles.title, { color: theme.colors.text }]}>Past Trends</Text>
 49:       <Text style={[styles.subtitle, { color: theme.colors.greyedOut }]}>30-day overview</Text>
 50: 
 51:       <View style={styles.grid}>
 52:         {trendOptions.map((opt) => {
 53:           const plantKey = keyMap[opt.value];
 54: 
 55:           const avgRaw = safeMonth?.[plantKey];
 56:           const avg = toSafeNumber(avgRaw, NaN);
 57: 
 58:           const avgLabel = Number.isFinite(avg) ? avg.toFixed(1) : '--';
 59: 
 60:           return (
 61:             <View
 62:               key={opt.value}
 63:               style={[styles.sparkCard, { backgroundColor: theme.colors.surface }]}>
 64:               {/* Header */}
 65:               <View style={styles.sparkHeader}>
 66:                 <Feather name={opt.icon as any} size={14} color={opt.color} />
 67:                 <Text style={[styles.sparkLabel, { color: theme.colors.text }]}>{opt.label}</Text>
 68:               </View>
 69: 
 70:               {/* 30-day sparkline */}
 71:               <LineChart
 72:                 data={getSparkFor(opt.value)}
 73:                 height={50}
 74:                 thickness={2}
 75:                 curved
 76:                 hideDataPoints
 77:                 hideRules
 78:                 hideYAxisText
 79:                 initialSpacing={0}
 80:                 spacing={6}
 81:                 color={opt.color}
 82:               />
 83: 
 84:               {/* Avg value */}
 85:               <Text style={[styles.sparkValue, { color: theme.colors.text }]}>
 86:                 {avgLabel} {opt.unit}
 87:               </Text>
 88:             </View>
 89:           );
 90:         })}
 91:       </View>
 92:     </View>
 93:   );
 94: }
