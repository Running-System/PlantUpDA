168:             style={styles.bgImage}
169:             resizeMode="cover"
170:           />
171:         </View>
172: 
173:         {/* HEADER */}
174:         <View style={styles.headerWrapper}>
175:           <View style={{ alignSelf: 'flex-start', marginBottom: 10 }}>
176:             <Text style={styles.brandText}>PlantUp</Text>
177:           </View>
178: 
179:           <MaskedView
180:             style={{ height: 100, width: '100%' }}
181:             maskElement={<Text style={styles.headerTitle}>Quests and{'\n'}Daily Rewards!</Text>}>
182:             <LinearGradient
183:               colors={['#FFFFFF', '#E0F7FA']}
184:               start={{ x: 0.2, y: 0 }}
185:               end={{ x: 0.8, y: 1 }}
186:               style={{ flex: 1 }}
187:             />
188:           </MaskedView>
189: 
190:           <View style={styles.wrapper}>
191:             <TimeLeft />
192:             <View style={styles.comingSoonBar}>
193:               <DailyStreakComponent></DailyStreakComponent>
194:             </View>
195:           </View>
196:         </View>
197: 
198:         {/* DRAGGABLE BOTTOM SHEET */}
199:         <DraggableBottomSheet initialHeight={0.55} maxHeight={0.75}>
200:           <View style={{ flex: 1, paddingHorizontal: 20 }}>
201:             {loading && !refreshing ? (
202:               <ActivityIndicator
203:                 size="large"
