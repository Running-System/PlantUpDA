 15: 
 16:   useEffect(() => {
 17:     const checkUrl = async () => {
 18:       const initialUrl = await Linking.getInitialURL();
 19: 
 20:       if (initialUrl && initialUrl.includes('access_token')) {
 21:         const { params } = QueryParams.getQueryParams(initialUrl);
 22:         const { access_token, refresh_token } = params;
 23: 
 24:         if (access_token && refresh_token) {
 25:           const { error } = await db.public.auth.setSession({
 26:             access_token,
 27:             refresh_token,
 28:           });
 29: 
 30:           if (error) {
 31:             console.error('Session restore failed:', error.message);
 32:           } else {
 33:             console.log('Session restored ‚Äî user can now reset password');
 34:           }
 35:         }
 36:       }
 37:     };
 38: 
 39:     checkUrl();
 40:   }, []);
 41: 
 42:   const handleUpdatePassword = async () => {
 43:     if (password.length < 6) {
 44:       setError('Password must be at least 6 characters');
 45:       return;
 46:     }
 47: 
 48:     setError('');
 49: 
 50:     const { error: updateError } = await db.public.auth.updateUser({
