151:     {
152:       title: 'Daily Quests',
153:       data: dailyQuests.length > 0 ? dailyQuests : [{ id: 'empty-daily', type: 'empty' } as any],
154:     },
155:     {
156:       title: 'Weekly Quests',
157:       data: weeklyQuests.length > 0 ? weeklyQuests : [{ id: 'empty-weekly', type: 'empty' } as any],
158:     },
159:   ];
160: 
161:   return (
162:     <GestureHandlerRootView style={{ flex: 1 }}>
163:       <View style={{ flex: 1 }}>
164:         {/* HINTERGRUND */}
