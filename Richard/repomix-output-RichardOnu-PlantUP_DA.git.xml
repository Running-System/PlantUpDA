205:     [HttpGet]
206:     public async Task<IActionResult> GetRandomVideos([FromQuery] string? category = null)
207:     {
208:         try
209:         {
210:             var token = GetTokenFromHeader();
211:             if (string.IsNullOrEmpty(token))
212:             {
213:                 return Unauthorized(new { error = "Please provide an authentication token" });
214:             }
215: 
216:             _logger.LogInformation("Authenticating user...");
217:             var user = await _sb.Client.Auth.GetUser(token);
218: 
219:             if (user == null)
220:             {
221:                 return Unauthorized(new { error = "Invalid token - please login again" });
222:             }
223:             
224:             // ‚úÖ Hole ALLE Videos und w√§hle dann zuf√§llig aus
225:             var query = _sb.Client.From<Post>()
226:                 .Where(x => x.Type == "video");
227: 
228:             if (!string.IsNullOrEmpty(category))
229:             {
230:                 query = query.Where(x => x.Category == category);
231:             }
232: 
233:             var allVideos = await query.Get();
234: 
235:             // ‚úÖ Zuf√§llige 3 Videos ausw√§hlen
236:             var random = new Random();
237:             var randomVideos = allVideos.Models
238:                 .OrderBy(x => random.Next())
239:                 .Take(3)
240:                 .ToList();
241: 
242:             // ‚úÖ Signed URLs erstellen
243:             var videoDtos = new List<VideoDto>();
244: 
245:             foreach (var video in randomVideos)
246:             {
247:                 var userClient = new SupabaseService("user_schema").Client;
248: 
249:                 var userResult = await userClient.From<User>()
250:                 .Filter("id", Supabase.Postgrest.Constants.Operator.Equals, video.UserId.ToString())
251:                 .Get();
252: 
253:                 var userAusUserTabelle = userResult.Models.FirstOrDefault();
254: 
255: 
256:                 var signedUrl = await CreateSignedUrl(video.MediaUrl);
257: 
258:                 videoDtos.Add(new VideoDto
259:                 {
260:                     Id = video.Id,
261:                     MediaUrl = signedUrl,
262:                     Caption = video.Caption,
263:                     Category = video.Category,
264:                     Likes = video.Likes,
265:                     Shares = video.Shares,
266:                     CommentsCount = video.CommentsCount,
267:                     Username = userAusUserTabelle?.Username ?? user?.Email ?? "Unknown User",
268:                 });
269:             }
270: 
271:             return Ok(new { videos = videoDtos });
272:         }
273:         catch (Exception ex)
274:         {
275:             _logger.LogError(ex, "Error fetching videos");
276:             return StatusCode(500, new { error = "Failed to fetch videos" });
277:         }
278:     }
279: 
280:     // ‚úÖ Signed URL f√ºr private Videos erstellen
281:     private async Task<string> CreateSignedUrl(string mediaUrl)
282:     {
283:         try
284:         {
285:             // Extrahiere den Dateipfad aus der URL
286:             var baseUrl = Environment.GetEnvironmentVariable("SUPABASE_URL");
287:             var prefix = $"{baseUrl}/storage/v1/object/public/post-media/";
288: 
289:             if (mediaUrl.StartsWith(prefix))
290:             {
291:                 var filePath = mediaUrl.Substring(prefix.Length);
292: 
293:                 // Erstelle eine Signed URL die 1 Stunde g√ºltig ist
294:                 var signedUrl = await _sb.Client.Storage
295:                     .From("post-media")
296:                     .CreateSignedUrl(filePath, 3600); // 1 Stunde G√ºltigkeit
297: 
298:                 return signedUrl ?? mediaUrl; // ‚úÖ Direkt den string zur√ºckgeben
299:             }
300: 
301:             return mediaUrl;
302:         }
303:         catch (Exception ex)
304:         {
305:             _logger.LogWarning($"Could not create signed URL: {ex.Message}");
306:             return mediaUrl; // Fallback zur original URL
307:         }
308:     }
