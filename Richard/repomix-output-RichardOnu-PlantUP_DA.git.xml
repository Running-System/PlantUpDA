 89: String scanNetworks()
 90: {
 91:   Serial.println("[Scan] Starting Wi-Fi scan...");
 92: 
 93:   WiFi.mode(WIFI_STA);
 94:   WiFi.disconnect(true);
 95:   delay(1000);
 96: 
 97:   int n = WiFi.scanNetworks(false, true);
 98:   Serial.printf("[Scan] Found %d networks.\n", n);
 99: 
100:   String options;
101:   if (n == 0)
102:   {
103:     options = "<option>No networks found</option>";
104:   }
105:   else
106:   {
107:     for (int i = 0; i < n; i++)
108:     {
109:       int rssi = WiFi.RSSI(i);
110:       if (rssi < -80)
111:         continue; // ‚ö° skip weak networks
112: 
113:       String ssidFound = WiFi.SSID(i);
114: 
115:       // Signal level 1‚Äì4
116:       int bars = 1;
117:       if (rssi > -50)
118:         bars = 4;
119:       else if (rssi > -60)
120:         bars = 3;
121:       else if (rssi > -70)
122:         bars = 2;
123: 
124:       // small inline signal bars
125:       String barsHtml = "<div class='signal'>";
126:       for (int b = 0; b < 4; b++)
127:         barsHtml += "<div class='bar" + String(b < bars ? " active" : "") + "'></div>";
128:       barsHtml += "</div>";
129: 
130:       options += "<div class='dropdown-item' onclick=\"selectSSID('" + ssidFound + "')\">" +
131:                  ssidFound + barsHtml + "</div>";
132:     }
133:   }
134: 
135:   WiFi.disconnect(true);
136:   return options;
137: }
