 52:     console.error('Abmeldung fehlgeschlagen:', error.message);
 53:   }
 54: };
 55: 
 56: export const LoginViaGoogle = async () => {
 57:   const redirectTo = makeRedirectUri({
 58:     scheme: 'plantup',
 59:     path: 'auth/reset-password',
 60:     isTripleSlashed: false,
 61:   });
 62:   const { data, error } = await db.public.auth.signInWithOAuth({
 63:     provider: 'google',
 64:     options: {
 65:       redirectTo,
 66:       skipBrowserRedirect: true,
 67:     },
 68:   });
 69: 
 70:   if (error) {
 71:     console.error('OAuth error:', error.message);
 72:     return;
 73:   }
 74: 
 75:   const res = await WebBrowser.openAuthSessionAsync(data?.url ?? '', redirectTo);
 76: 
 77:   if (res.type === 'success' && res.url) {
 78:     const { params } = QueryParams.getQueryParams(res.url);
 79:     const { access_token, refresh_token } = params;
 80: 
 81:     if (access_token && refresh_token) {
 82:       const { error: sessionError } = await db.public.auth.setSession({
 83:         access_token,
 84:         refresh_token,
 85:       });
 86: 
 87:       if (sessionError) {
 88:         console.error('Session error:', sessionError.message);
 89:       } else {
 90:         console.log('Logged in successfully with Google');
 91:       }
 92:     }
 93:   } else {
 94:     console.warn('Login canceled or failed');
 95:   }
 96: 
 97:   const {
 98:     data: { user },
 99:   } = await db.public.auth.getUser();
100: 
101:   const { data: existingUser, error: selectError } = await db.user
102:     .from('Users')
103:     .select('id')
104:     .eq('id', user?.id)
105:     .maybeSingle(); // ‚Üê returns data: null when no row found (no error!)
106: 
107:   if (selectError) {
108:     console.warn('Failed to check if user exists:', selectError);
109:     return null;
110:   }
111: 
112:   // 2Ô∏è‚É£ User already exists ‚Üí skip inserting
113:   if (existingUser) {
114:     console.log('User already exists:', existingUser.id);
115:     return existingUser;
116:   }
117: 
118:   // 3Ô∏è‚É£ Insert new user
119:   const { error: insertError } = await db.user
120:     .from('Users')
121:     .insert([{ id: user?.id, username: user?.email }]);
122: 
123:   if (insertError) {
124:     console.warn('Failed to insert user:', insertError);
125:     return null;
126:   }
127: 
128:   return user;
129: };
