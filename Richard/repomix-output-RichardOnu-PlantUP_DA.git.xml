<file path="MqttToDatabase/MqttToMiddlewareClient/Program.cs">
  1: using DotNetEnv; 
  2: using MQTTnet;
  3: using MQTTnet.Protocol;
  4: using Supabase;
  5: using Supabase.Interfaces;
  6: using Supabase.Postgrest.Attributes;
  7: using Supabase.Postgrest.Models;
  8: using System.Buffers;
  9: using System.Text;
 10: using System.Text.Json;
 11: using static System.Runtime.InteropServices.JavaScript.JSType;
 12: 
 13: 
 14: class Program
 15: {
 16:     static List<object> dataBuffer = new();
 17:     private static Client supabaseClient = null!;
 18: 
 19:     [Table("Controllers")]
 20:     public class SensorData : BaseModel
 21:     {
 22:         [PrimaryKey("id", false)]
 23:         [Column("id")] public Guid Id { get; set; }
 24:         [Column("light")] public double Light { get; set; }
 25:         [Column("temperature")] public double Temperature { get; set; }
 26:         [Column("humidity")] public double Humidity { get; set; }
 27:         [Column("soil_moisture")] public double SoilMoisture { get; set; }
 28:         [Column("electrical_conductivity")] public double ElectricalConductivity { get; set; }
 29:         [Column("user_id")] public Guid UserId { get; set; }
 30:         [Column("plant_id")] public Guid PlantId { get; set; }
 31:     }
 32: 
 33:     static async Task Main()
 34:     {
 35:         DotNetEnv.Env.Load("../../../.env");
 36:         //mosquitto_pub -h localhost -p 1883 -t plantup/sensor -m "{\"user_id\":\"939ad4a0-7ded-4155-a990-5b4ea946d14f\",\"plant_id\":\"ae85c3b5-7075-4fd7-a209-19bb8ca5e873\",\"light\":351.5,\"temperature\":24.6,\"humidity\":55.8,\"soil_moisture\":45.0,\"electrical_conductivity\":2.3}"
 37: 
 38:         // üîπ Supabase vorbereiten
 39:         string supabaseUrl = Environment.GetEnvironmentVariable("SUPABASE_URL")!;
 40:         string supabaseKey = Environment.GetEnvironmentVariable("SUPABASE_KEY")!;
 41: 
 42:        
 43:         var options = new SupabaseOptions { Schema = "microcontroller_schema" };
 44:         supabaseClient = new Client(supabaseUrl, supabaseKey, options);
 45:         await supabaseClient.InitializeAsync();
 46:         Console.WriteLine("Mit Supabase verbunden!");
 47: 
 48:         // 1Ô∏è‚É£ Factory und Client erstellen
 49:         var mqttFactory = new MqttClientFactory();
 50:         using var mqttClient = mqttFactory.CreateMqttClient();
 51: 
 52:         // 2Ô∏è‚É£ Optionen konfigurieren (Mosquitto l√§uft lokal)
 53:         var mqttClientOptions = new MqttClientOptionsBuilder()
 54:             .WithTcpServer("localhost", 1883)
 55:             .WithClientId("PlantUpClient_" + Guid.NewGuid())
 56:             .WithCleanSession()
 57:             .Build();
 58: 
 59:         //3Ô∏è‚É£ Nachrichtenempfang einrichten
 60:         mqttClient.ApplicationMessageReceivedAsync += async e =>
 61:         {           
 62:             try
 63:             {
 64:                 // Decode MQTT payload
 65:                 string jsonString = Encoding.UTF8.GetString(e.ApplicationMessage.Payload.ToArray());
 66:                 Console.WriteLine($"Nachricht empfangen:\nTopic: {e.ApplicationMessage.Topic}\nPayload: {jsonString}");
 67: 
 68:                 // Deserialize JSON into dynamic object
 69:                 var payload = JsonSerializer.Deserialize<JsonElement>(jsonString);
 70: 
 71:                 // Create structured object (like JS dataBuffer.push({...}))
 72:                 var entry = new SensorData
 73:                 {
 74:                     UserId = Guid.Parse(payload.GetProperty("user_id").GetString()!),
 75:                     Light = payload.GetProperty("light").GetDouble(),
 76:                     Temperature = payload.GetProperty("temperature").GetDouble(),
 77:                     SoilMoisture = payload.GetProperty("soil_moisture").GetDouble(),
 78:                     Humidity = payload.GetProperty("humidity").GetDouble(),
 79:                     ElectricalConductivity = payload.GetProperty("electrical_conductivity").GetDouble(),
 80:                     PlantId = Guid.Parse(payload.GetProperty("plant_id").GetString()!)
 81:                 };
 82: 
 83:                 dataBuffer.Add(entry);
 84:                 Console.WriteLine("Sende an Supabase...");
 85: 
 86:                 // Direkt an Supabase schicken:
 87:                 var result = await supabaseClient
 88:                     .From<SensorData>() 
 89:                     .Insert(entry);
 90: 
 91: 
 92:                 Console.WriteLine("Erfolgreich in Supabase gespeichert!");
 93:             }
 94:             catch (Exception ex)
 95:             {
 96:                 Console.WriteLine($"Fehler beim Verarbeiten der Nachricht: {ex.Message}");
 97:             }
 98:         };      
 99: 
100:         // 4Ô∏è‚É£ Verbinden mit dem Broker
101:         Console.WriteLine(" Verbinde mit Mosquitto...");
102:         var response = await mqttClient.ConnectAsync(mqttClientOptions, CancellationToken.None);
103:         Console.WriteLine($"Verbunden! Ergebnis: {response.ResultCode}");
104: 
105: 
106:         // 5Ô∏è‚É£ Topic abonnieren
107:         var topic = "plantup/sensor";
108:         await mqttClient.SubscribeAsync(
109:             mqttFactory.CreateSubscribeOptionsBuilder()
110:                 .WithTopicFilter(f => f.WithTopic(topic))
111:                 .Build(),
112:             CancellationToken.None);
113: 
114: 
115:         //// 7Ô∏è‚É£ Sauber trennen
116:         Console.ReadLine();
117:         await mqttClient.DisconnectAsync();
118: 
119:     }
120: }
</file>

