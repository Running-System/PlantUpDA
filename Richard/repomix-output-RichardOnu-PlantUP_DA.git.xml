170:     private async Task<string> UploadToSupabaseStorage(string filePath, string fileName, string userToken, string userId)
171:     {
172:         var supabaseUrl = Environment.GetEnvironmentVariable("SUPABASE_URL");
173:         var bucketName = "post-media";
174: 
175:         // ‚úÖ Korrekte URL-Bereinigung
176:         var baseUrl = supabaseUrl?.TrimEnd('/') ?? throw new Exception("SUPABASE_URL is not set");
177:         var userFolderPath = $"{userId}/{fileName}";
178: 
179:         // ‚úÖ Korrekte Upload URL
180:         var uploadUrl = $"{baseUrl}/storage/v1/object/{bucketName}/{userFolderPath}";
181: 
182:         _logger.LogInformation($"üîç DEBUG - Upload URL: {uploadUrl}"); // ‚úÖ Debug
183: 
184:         using var http = new HttpClient();
185:         http.DefaultRequestHeaders.Authorization = new AuthenticationHeaderValue("Bearer", userToken);
186: 
187:         await using var fileStream = System.IO.File.OpenRead(filePath);
188:         using var content = new StreamContent(fileStream);
189:         content.Headers.ContentType = new MediaTypeHeaderValue("video/mp4");
190: 
191:         var response = await http.PostAsync(uploadUrl, content);
192: 
193:         if (!response.IsSuccessStatusCode)
194:         {
195:             var error = await response.Content.ReadAsStringAsync();
196:             throw new Exception($"Storage upload failed ({response.StatusCode}): {error}");
197:         }
198: 
199:         // ‚úÖ Korrekte Public URL ohne doppelten Slash
200:         var publicUrl = $"{baseUrl}/storage/v1/object/public/{bucketName}/{userFolderPath}";
201:         _logger.LogInformation($"üîç DEBUG - Final Public URL: {publicUrl}"); // ‚úÖ Debug
202:         return publicUrl;
203:     }
