63: bool WiFiManager::testConnection(const String &testSsid, const String &testPass)
64: {
65:     WiFi.mode(WIFI_STA);
66:     WiFi.begin(testSsid.c_str(), testPass.c_str());
67:     unsigned long start = millis();
68:     while (WiFi.status() != WL_CONNECTED && millis() - start < 8000)
69:     {
70:         delay(500);
71:         Serial.print(".");
72:     }
73:     bool ok = WiFi.status() == WL_CONNECTED;
74:     WiFi.disconnect(true);
