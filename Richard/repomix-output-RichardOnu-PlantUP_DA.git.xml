 39: type UserQuest = {
 40:   id: number;
 41:   status: 'active' | 'completed' | 'claimed';
 42:   progress: number;
 43:   plant_id: string | null;
 44:   Plants: { nickname: string } | null;
 45:   Quests: QuestData;
 46: };
