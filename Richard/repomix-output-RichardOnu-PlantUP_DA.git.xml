  1: #include <WiFi.h>
  2: #include <PubSubClient.h>
  3: #include <Wire.h>
  4: #include <BH1750.h>
  5: #include <Adafruit_MLX90614.h>
  6: #include <esp_sleep.h>
  7: 
  8: 
  9: // --- Wi-Fi credentials ---
 10: const char* ssid = "YOUR_WIFI_SSID";
 11: const char* password = "YOUR_WIFI_PASSWORD";
 12: 
 13: // --- MQTT broker info ---
 14: const char* mqtt_server = "192.168.1.100";   // IP Address of the MQTT BROKER
 15: const int mqtt_port = 1883;
 16: const char* mqtt_topic = "plantup/sensor";
 17: 
 18: // --- Supabase IDs (same as your .NET example) ---
 19: const char* user_id = "939ad4a0-7ded-4155-a990-5b4ea946d14f";
 20: const char* plant_id = "ae85c3b5-7075-4fd7-a209-19bb8ca5e873";
 21: 
 22: // --- Sensor pins ---
 23: #define SOIL_ANALOG_PIN 1   // AO from LM393
 24: #define SOIL_DIGITAL_PIN 2  // DO from LM393
 25: 
 26: // --- ESP32 Deep Sleep ---
 27: #define uS_TO_S_FACTOR 1000000ULL  // Conversion factor for seconds
 28: #define TIME_TO_SLEEP  600          // 10 minutes = 600 seconds
 29: 
 30: 
 31: 
 32: // --- Sensors ---
 33: BH1750 lightMeter;
 34: Adafruit_MLX90614 mlx = Adafruit_MLX90614();
 35: 
 36: WiFiClient espClient;
 37: PubSubClient client(espClient);
 38: 
 39: // --- WiFi connection ---
 40: void setup_wifi() {
 41:   delay(10);
 42:   Serial.println();
 43:   Serial.printf("Connecting to %s", ssid);
 44: 
 45:   WiFi.begin(ssid, password);
 46:   while (WiFi.status() != WL_CONNECTED) {
 47:     delay(500);
 48:     Serial.print(".");
 49:   }
 50: 
 51:   Serial.println("\n‚úÖ WiFi connected!");
 52:   Serial.print("IP address: ");
 53:   Serial.println(WiFi.localIP());
 54: }
 55: 
 56: // --- MQTT reconnect ---
 57: void reconnect() {
 58:   while (!client.connected()) {
 59:     Serial.print("Attempting MQTT connection...");
 60:     if (client.connect("ESP32_PlantUpPublisher")) {
 61:       Serial.println("‚úÖ connected");
 62:     } else {
 63:       Serial.print("‚ùå failed, rc=");
 64:       Serial.print(client.state());
 65:       Serial.println(" try again in 5s");
 66:       delay(5000);
 67:     }
 68:   }
 69: }
 70: 
 71: void setup() {
 72: 
 73:   esp_sleep_wakeup_cause_t wakeup_reason = esp_sleep_get_wakeup_cause();
 74:   Serial.printf("Wakeup reason: %d\n", wakeup_reason);
 75: 
 76:   Serial.begin(115200);
 77:   delay(1000);
 78:   Serial.println("üåø Starting PlantUp MQTT publisher...");
 79: 
 80:   setup_wifi();
 81:   client.setServer(mqtt_server, mqtt_port);
 82: 
 83:   Wire.begin(8, 9); // SDA, SCL
 84: 
 85:   // Initialize sensors
 86:   if (lightMeter.begin(BH1750::CONTINUOUS_HIGH_RES_MODE))
 87:     Serial.println("‚úÖ BH1750 ready");
 88:   else
 89:     Serial.println("‚ùå BH1750 not detected!");
 90: 
 91:   if (mlx.begin())
 92:     Serial.println("‚úÖ MLX90614 ready");
 93:   else {
 94:     Serial.println("‚ùå MLX90614 not detected!");
 95:     while (1);
 96:   }
 97: 
 98:   pinMode(SOIL_ANALOG_PIN, INPUT);
 99:   pinMode(SOIL_DIGITAL_PIN, INPUT);
100: }
101: 
102: void loop() {
103:   if (!client.connected()) reconnect();
104:   client.loop();
105: 
106:   // --- Read sensors ---
107:   float lux = lightMeter.readLightLevel();
108:   double temp = mlx.readObjectTempC();
109:   int soilRaw = analogRead(SOIL_ANALOG_PIN);
110:   float soilPercent = map(soilRaw, 4095, 0, 0, 100);  // invert scale
111: 
112:   // --- Construct JSON payload ---
113:   String payload = "{";
114:   payload += "\"user_id\":\"" + String(user_id) + "\",";
115:   payload += "\"plant_id\":\"" + String(plant_id) + "\",";
116:   payload += "\"light\":" + String(lux, 2) + ",";
117:   payload += "\"temperature\":" + String(temp, 2) + ",";
118:   payload += "\"humidity\":0.0,";  // no humidity sensor, keep placeholder
119:   payload += "\"soil_moisture\":" + String(soilPercent, 1) + ",";
120:   payload += "\"electrical_conductivity\":0.0"; // placeholder
121:   payload += "}";
122: 
123:   Serial.println("üì§ Publishing:");
124:   Serial.println(payload);
125: 
126:   // --- Publish ---
127:   client.publish(mqtt_topic, payload.c_str());
128: 
129:   // delay(10000); // send every 5s
130: 
131:   Serial.println("Going to sleep now...");
132:   esp_sleep_enable_timer_wakeup(TIME_TO_SLEEP * uS_TO_S_FACTOR);
133:   esp_deep_sleep_start();
134: 
135: 
136: }
</file>

