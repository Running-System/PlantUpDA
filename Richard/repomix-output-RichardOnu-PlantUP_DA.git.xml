 66: // Fetch ONLY the latest row (very fast)
 67: async function fetchLatestController(plantId: string): Promise<PlantController | null> {
 68:   const { data, error } = await db.micro
 69:     .from('Controllers')
 70:     .select('id, temperature, humidity, soil_moisture, light, electrical_conductivity, created_at')
 71:     .eq('plant_id', plantId)
 72:     .order('created_at', { ascending: false })
 73:     .limit(1)
 74:     .maybeSingle(); // maybeSingle is safer than single if no data exists
 75: 
 76:   if (error) console.error('Error fetching latest:', error);
 77:   return data as PlantController;
 78: }
