<file path="PlantUp/src/components/DraggableBottomSheet.tsx">
  1: import React, { useEffect } from 'react';
  2: import { View, Dimensions } from 'react-native';
  3: import { StyleSheet, useUnistyles } from 'react-native-unistyles';
  4: import { GestureDetector, Gesture } from 'react-native-gesture-handler';
  5: import Animated, { useSharedValue, useAnimatedStyle, withSpring } from 'react-native-reanimated';
  6: 
  7: const { height: SCREEN_HEIGHT } = Dimensions.get('window');
  8: 
  9: type BottomSheetProps = {
 10:   children?: React.ReactNode;
 11:   initialHeight?: number; // Z.B. 0.55 (55% des Screens)
 12:   maxHeight?: number; // Z.B. 0.90 (90% des Screens)
 13: };
 14: 
 15: export default function DraggableBottomSheet({
 16:   children,
 17:   initialHeight = 0.55,
 18:   maxHeight = 0.9,
 19: }: BottomSheetProps) {
 20:   const { theme } = useUnistyles();
 21:   // BERECHNUNG DER GRENZEN (Prozentual)
 22:   const START_Y = -SCREEN_HEIGHT * initialHeight;
 23:   const MAX_Y = -SCREEN_HEIGHT * maxHeight;
 24: 
 25:   const translateY = useSharedValue(START_Y);
 26:   const context = useSharedValue({ y: 0 });
 27: 
 28:   useEffect(() => {
 29:     translateY.value = withSpring(START_Y, { damping: 50 });
 30:   }, [initialHeight, START_Y]);
 31: 
 32:   const gesture = Gesture.Pan()
 33:     .onStart(() => {
 34:       context.value = { y: translateY.value };
 35:     })
 36:     .onUpdate((event) => {
 37:       let newY = event.translationY + context.value.y;
 38:       // Clamping: Nicht h√∂her als MAX und nicht tiefer als START
 39:       newY = Math.max(newY, MAX_Y);
 40:       newY = Math.min(newY, START_Y);
 41:       translateY.value = newY;
 42:     })
 43:     .onEnd(() => {
 44:       const threshold = START_Y + (MAX_Y - START_Y) / 3;
 45:       if (translateY.value < threshold) {
 46:         translateY.value = withSpring(MAX_Y, { damping: 50 });
 47:       } else {
 48:         translateY.value = withSpring(START_Y, { damping: 50 });
 49:       }
 50:     });
 51: 
 52:   const rBottomSheetStyle = useAnimatedStyle(() => {
 53:     return {
 54:       transform: [{ translateY: translateY.value }],
 55:     };
 56:   });
 57: 
 58:   return (
 59:     <Animated.View style={[styles.bottomSheetContainer, rBottomSheetStyle]}>
 60:       {/* 1. Der Griff (GestureDetector) */}
 61:       <GestureDetector gesture={gesture}>
 62:         <View style={styles.lineWrapper}>
 63:           <View style={styles.line} />
 64:         </View>
 65:       </GestureDetector>
 66: 
 67:       {/* 2. Der Inhalt (Frei f√ºr ScrollView) */}
 68:       <View style={styles.contentContainer}>{children}</View>
 69:     </Animated.View>
 70:   );
 71: }
 72: 
 73: const styles = StyleSheet.create((theme) => ({
 74:   bottomSheetContainer: {
 75:     height: SCREEN_HEIGHT,
 76:     width: '100%',
 77:     backgroundColor: theme.colors.surface,
 78:     position: 'absolute',
 79:     top: SCREEN_HEIGHT,
 80:     borderRadius: 30,
 81:     shadowColor: '#000',
 82:     shadowOffset: { width: 0, height: -3 },
 83:     shadowOpacity: 0.1,
 84:     shadowRadius: 5,
 85:     elevation: 10,
 86:   },
 87:   lineWrapper: {
 88:     width: '100%',
 89:     height: 60, // Gro√üe Touch-Area
 90:     alignItems: 'center',
 91:     justifyContent: 'center',
 92:     backgroundColor: 'transparent',
 93:   },
 94:   line: {
 95:     width: 50,
 96:     height: 5,
 97:     backgroundColor: theme.colors.greyedOut,
 98:     borderRadius: 3,
 99:   },
100:   contentContainer: {
101:     flex: 1,
102:     marginBottom: 100,
103:   },
104: }));
</file>

