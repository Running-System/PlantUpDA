  9: static const char *CHAR_DATA_UUID = "7c3b0002-1cbe-4e4e-9e8a-5b5d3e8a9f01";
 10: 
 11: // ====== STATE ======
 12: NimBLEServer *pServer = nullptr;
 13: NimBLECharacteristic *pDataChar = nullptr;
 14: bool deviceConnected = false;
 15: bool notificationsEnabled = false;
 16: 
 17: const char *ssid = "Abudi";
 18: const char *password = "asimasim2";
 19: 
 20: // NTP settings
 21: const char *ntpServer = "pool.ntp.org";
 22: const long gmtOffset_sec = 0;     // UTC
 23: const int daylightOffset_sec = 0; // add +3600 if DST
 24: 
 25: #define SOIL_ANALOG_PIN 1
 26: #define SOIL_DIGITAL_PIN 2
 27: #define I2C_SDA_PIN 8
 28: #define I2C_SCL_PIN 9
 29: 
 30: BH1750 lightMeter;
 31: 
 32: void connectWiFiAndSyncTime()
 33: {
 34:   Serial.printf("[WiFi] Connecting to %s", ssid);
 35:   WiFi.begin(ssid, password);
 36: 
 37:   unsigned long startAttempt = millis();
 38:   while (WiFi.status() != WL_CONNECTED)
 39:   {
 40:     Serial.print(".");
 41:     delay(500);
 42: 
 43:     if (millis() - startAttempt > 15000)
 44:     {
 45:       Serial.println("\n[WiFi] Failed to connect!");
 46:       return;
 47:     }
 48:   }
 49: 
 50:   Serial.println("\n[WiFi] Connected!");
 51:   Serial.print("[WiFi] IP Address: ");
 52:   Serial.println(WiFi.localIP());
 53: 
 54:   // Synchronize time via NTP
 55:   configTime(gmtOffset_sec, daylightOffset_sec, ntpServer);
 56:   Serial.println("[NTP] Waiting for time...");
 57: 
 58:   struct tm timeinfo;
 59:   while (!getLocalTime(&timeinfo))
 60:   {
 61:     Serial.print(".");
 62:     delay(500);
 63:   }
 64: 
 65:   Serial.println("\n[NTP] Time synchronized!");
 66:   Serial.printf("[NTP] %02d:%02d:%02d\n", timeinfo.tm_hour, timeinfo.tm_min, timeinfo.tm_sec);
 67: }
 68: 
 69: // ----------------------------------------------------
 70: // 1. BLE CALLBACKS
 71: // ----------------------------------------------------
 72: class ServerCallbacks : public NimBLEServerCallbacks
 73: {
 74:   void onConnect(NimBLEServer *pServer) override
 75:   {
 76:     deviceConnected = true;
 77:     Serial.println("[BLE] Central connected");
 78:   }
 79: 
 80:   void onDisconnect(NimBLEServer *pServer) override
 81:   {
 82:     deviceConnected = false;
 83:     notificationsEnabled = false;
 84:     Serial.println("[BLE] Central disconnected");
 85:     NimBLEDevice::startAdvertising();
 86:   }
 87: };
 88: 
 89: class CharacteristicCallbacks : public NimBLECharacteristicCallbacks
 90: {
 91:   void onSubscribe(NimBLECharacteristic *c, ble_gap_conn_desc *desc, uint16_t subValue) override
 92:   {
 93:     notificationsEnabled = (subValue & 0x0001) || (subValue & 0x0002);
 94:     Serial.printf("[BLE] Notifications %s\n", notificationsEnabled ? "ENABLED" : "DISABLED");
 95:   }
 96: };
 97: 
 98: // ----------------------------------------------------
 99: // 2. BLE INITIALIZATION
100: // ----------------------------------------------------
101: void initBLE()
102: {
103:   NimBLEDevice::init(DEVICE_NAME);
104:   NimBLEDevice::setSecurityAuth(true, false, false);
105:   NimBLEDevice::setPower(ESP_PWR_LVL_P9);
106: 
107:   Serial.println("[BLE] Device initialized");
108: }
109: 
110: void createServiceAndCharacteristic()
111: {
112:   pServer = NimBLEDevice::createServer();
113:   pServer->setCallbacks(new ServerCallbacks());
114: 
115:   NimBLEService *service = pServer->createService(SERVICE_UUID);
116:   pDataChar = service->createCharacteristic(
117:       CHAR_DATA_UUID,
118:       NIMBLE_PROPERTY::READ | NIMBLE_PROPERTY::NOTIFY);
119:   pDataChar->setCallbacks(new CharacteristicCallbacks());
120:   service->start();
121: 
122:   Serial.println("[BLE] Service + characteristic created");
123: }
124: 
125: void startAdvertising()
126: {
127:   NimBLEAdvertising *adv = NimBLEDevice::getAdvertising();
128:   adv->addServiceUUID(SERVICE_UUID);
129:   adv->setScanResponse(true);
130:   adv->start();
131:   Serial.println("[BLE] Advertising started");
132: }
133: 
134: // ----------------------------------------------------
135: // 3. DATA SENDER (Notify)
136: // ----------------------------------------------------
137: void sendFixedData()
138: {
139:   if (!deviceConnected || !notificationsEnabled)
140:     return;
141: 
142:   // --- Read sensors ---
143:   float lightLux = 0.0f;
144:   if (lightMeter.measurementReady(true))
145:   {
146:     lightLux = lightMeter.readLightLevel();
147:   }
148:   else
149:   {
150:     // fallback if not ready
151:     lightLux = 0.0f;
152:   }
153: 
154:   int soilAnalog = analogRead(SOIL_ANALOG_PIN);
155:   int soilDigital = digitalRead(SOIL_DIGITAL_PIN);
156: 
157:   // Convert analog value (0‚Äì4095 for ESP32 ADC) to percentage (0‚Äì100)
158:   float soilPercent = 100.0f - ((soilAnalog / 4095.0f) * 100.0f);
159: 
160:   // --- Timestamp ---
161:   time_t now;
162:   time(&now);
163:   unsigned long long ts_ms = (unsigned long long)now * 1000ULL;
164: 
165:   // --- JSON Payload ---
166:   char json[180];
167:   snprintf(json, sizeof(json),
168:            "{\"soil_percent\":%.1f,"
169:            "\"soil_raw\":%d,"
170:            "\"soil_digital\":%d,"
171:            "\"light_lx\":%.1f,"
172:            "\"ts_unix\":%llu}",
173:            soilPercent,
174:            soilAnalog,
175:            soilDigital,
176:            lightLux,
177:            ts_ms);
178: 
179:   // --- Send via BLE ---
180:   pDataChar->setValue((uint8_t *)json, strlen(json));
181:   pDataChar->notify();
182: 
183:   // --- Debug log ---
184:   Serial.print("[BLE] ‚Üí ");
185:   Serial.println(json);
186: }
187: 
188: // ----------------------------------------------------
189: // 4. MAIN
190: // ----------------------------------------------------
191: void setup()
192: {
193:   Serial.begin(115200);
194:   delay(500);
195:   Serial.println("\n[BOOT] PlantUp Env BLE demo (real sensors)");
196: 
197:   // --- I2C + BH1750 setup ---
198:   Wire.begin(I2C_SDA_PIN, I2C_SCL_PIN);
199:   if (lightMeter.begin(BH1750::CONTINUOUS_HIGH_RES_MODE))
200:   {
201:     Serial.println("[BH1750] Sensor ready");
202:   }
203:   else
204:   {
205:     Serial.println("[BH1750] Not detected, check wiring!");
206:   }
207: 
208:   // --- Soil sensor setup ---
209:   pinMode(SOIL_ANALOG_PIN, INPUT);
210:   pinMode(SOIL_DIGITAL_PIN, INPUT);
211: 
212:   connectWiFiAndSyncTime();
213:   initBLE();
214:   createServiceAndCharacteristic();
215:   startAdvertising();
216: }
217: 
218: void loop()
219: {
220:   static unsigned long lastSend = 0;
221:   unsigned long now = millis();
222: 
223:   // Send new data every second when subscribed
224:   if (now - lastSend >= 1000)
225:   {
226:     lastSend = now;
227:     sendFixedData();
228:   }
229: 
230:   delay(5);
231: }
</file>

