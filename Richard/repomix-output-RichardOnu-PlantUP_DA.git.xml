  1: // === PlantUp Enclosure with Side Wall Tube Port (√ò6mm Stainless Steel Tube) ===
  2: // Adds a side wall hole for a 6mm tube, adjustable height position.
  3: 
  4: // ---------------------------
  5: // üì¶ Dimensions & Parameters
  6: // ---------------------------
  7: pcb_x = 87;
  8: pcb_y = 49;
  9: pcb_z = 1.6;
 10: 
 11: wall = 2;
 12: clearance = 10;
 13: 
 14: standoff_h = 6;
 15: standoff_r = 3;
 16: hole_r = 1.6;              // M3 clearance radius (~3.2mm)
 17: mount_offset = 3;
 18: wall_height_extra = 20;
 19: lid_thickness = 3;
 20: corner_r = 4;
 21: 
 22: screw_r = 1.6;
 23: boss_r = 3.8;
 24: boss_height = 8;
 25: 
 26: // üåø Tube parameters
 27: tube_diameter = 6;          // stainless steel tube √ò6mm
 28: tube_wall_offset = 2;       // inset from outer surface (kept for future use)
 29: tube_height = 25;           // <--- adjustable height (center from bottom)
 30: tube_side = "right";        // "left" or "right"
 31: 
 32: // üåø Soil Moisture Probe Slits (LM393 fork sensor)
 33: slit_width   = 2.2;         // fork (PCB) thickness (+ tolerance) ‚Äî along Y
 34: slit_spacing = 17;          // distance between probe legs (center-to-center)
 35: slit_length  = 60;          // vertical (Z) slot length
 36: slit_depth   = wall + 1;    // cut depth through wall
 37: slits_height = 20;          // vertical center of slits from bottom
 38: slits_side   = "right";     // "left" or "right"
 39: 
 40: // ---------------------------
 41: // ‚öôÔ∏è Box Dimensions
 42: // ---------------------------
 43: inner_x = pcb_x + clearance;
 44: inner_y = pcb_y + clearance;
 45: inner_z = pcb_z + clearance + standoff_h + wall_height_extra;
 46: 
 47: outer_x = inner_x + 2 * wall;
 48: outer_y = inner_y + 2 * wall;
 49: outer_z = inner_z + wall;
 50: 
 51: // Corner screw boss coordinates
 52: corner_positions = [
 53:     [boss_r + wall, boss_r + wall],
 54:     [outer_x - boss_r - wall, boss_r + wall],
 55:     [boss_r + wall, outer_y - boss_r - wall],
 56:     [outer_x - boss_r - wall, outer_y - boss_r - wall]
 57: ];
 58: 
 59: // ---------------------------
 60: // üîß Helpers
 61: // ---------------------------
 62: function wall_x(side) = (side == "right") ? (outer_x - wall) : 0;
 63: function center_y()   = outer_y / 2;
 64: 
 65: // ---------------------------
 66: // üß± Bottom Box with Tube Hole & Slits
 67: // ---------------------------
 68: module bottom_box() {
 69:     difference() {
 70:         // --- Outer rounded shell ---
 71:         color([0.0, 1.0, 1.0, 0.25])
 72:         hull() {
 73:             for (x = [0, outer_x - corner_r * 2])
 74:                 for (y = [0, outer_y - corner_r * 2])
 75:                     translate([x + corner_r, y + corner_r, 0])
 76:                         cylinder(h = outer_z, r = corner_r, $fn = 60);
 77:         }
 78: 
 79:         // --- Inner hollow space ---
 80:         translate([wall, wall, wall])
 81:             cube([inner_x, inner_y, inner_z]);
 82: 
 83:         // --- 6mm tube hole in selected wall ---
 84:         translate([wall_x(tube_side), center_y(), tube_height])
 85:             rotate([0, 90, 0])
 86:                 cylinder(h = wall + 1, r = tube_diameter / 2, $fn = 80);
 87: 
 88:         // --- Rectangular probe slits (two slots), centered by slit_spacing ---
 89:         // NOTE: slit_width along Y, slit_length along Z
 90:         // Upper slot (higher Y)
 91:         translate([wall_x(slits_side),
 92:                    center_y() + (slit_spacing/2) - (slit_width/2),
 93:                    slits_height - (slit_length/2)])
 94:             rotate([0, 90, 0])
 95:                 cube([slit_depth, slit_width, slit_length]);
 96: 
 97:         // Lower slot (lower Y)
 98:         translate([wall_x(slits_side),
 99:                    center_y() - (slit_spacing/2) - (slit_width/2),
100:                    slits_height - (slit_length/2)])
101:             rotate([0, 90, 0])
102:                 cube([slit_depth, slit_width, slit_length]);
103:     }
104: 
105:     // === Screw bosses (dark blue) ===
106:     color([0.0, 0.0, 0.5, 0.3])
107:     for (pos = corner_positions)
108:         translate([pos[0], pos[1], outer_z - boss_height])
109:         difference() {
110:             cylinder(h = boss_height, r = boss_r, $fn = 50);
111:             cylinder(h = boss_height + 1, r = screw_r, $fn = 40);
112:         }
113: 
114:     // === PCB standoffs (yellow) ===
115:     color([1.0, 1.0, 0.0, 0.8])
116:     for (h = [
117:         [mount_offset, mount_offset],
118:         [pcb_x - mount_offset, mount_offset],
119:         [mount_offset, pcb_y - mount_offset],
120:         [pcb_x - mount_offset, pcb_y - mount_offset]
121:     ])
122:         translate([
123:             wall + (inner_x - pcb_x) / 2 + h[0],
124:             wall + (inner_y - pcb_y) / 2 + h[1],
125:             wall
126:         ])
127:         difference() {
128:             cylinder(h = standoff_h, r = standoff_r, $fn = 40);
