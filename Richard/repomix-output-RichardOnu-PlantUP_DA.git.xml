152:     private string GetTokenFromHeader()
153:     {
154:         try
155:         {
156:             var authHeader = Request.Headers["Authorization"].ToString();
157:             if (!string.IsNullOrEmpty(authHeader) && authHeader.StartsWith("Bearer "))
158:             {
159:                 return authHeader.Substring(7);
160:             }
161:             return null;
162:         }
163:         catch (Exception ex)
164:         {
165:             _logger.LogError(ex, "Error reading authorization header");
166:             return null;
167:         }
168:     }
