  9: namespace VideoAPI.Controllers;
 10: 
 11: [ApiController]
 12: [Route("api/[controller]")]
 13: public class VideosController : ControllerBase
 14: {
 15:     private readonly SupabaseService _sb;
 16:     private readonly VideoProcessor _processor;
 17:     private readonly ILogger<VideosController> _logger;
 18: 
 19:     public VideosController(SupabaseService sb, VideoProcessor processor, ILogger<VideosController> logger)
 20:     {
 21:         _sb = sb;
 22:         _processor = processor;
 23:         _logger = logger;
 24:     }
 25: 
 26:     public class VideoUploadModel
 27:     {
 28:         public IFormFile Video { get; set; }
 29:         public string? Caption { get; set; }
 30:         public string? Category { get; set; }
 31:     }
 32: 
 33:     [HttpPost]
 34:     [RequestSizeLimit(200_000_000)]
 35:     [Consumes("multipart/form-data")]
 36:     public async Task<IActionResult> Upload([FromForm] VideoUploadModel model)
 37:     {
 38:         _logger.LogInformation("Video upload started");
 39: 
 40:         // ‚úÖ Token Validation
 41:         var token = GetTokenFromHeader();
 42:         if (string.IsNullOrEmpty(token))
 43:         {
 44:             return Unauthorized(new { error = "Please provide an authentication token" });
 45:         }
 46: 
 47:         // ‚úÖ File Validation
 48:         if (model.Video == null || model.Video.Length == 0)
 49:         {
 50:             return BadRequest(new { error = "No video file provided" });
 51:         }
 52: 
 53:         if (model.Video.ContentType != "video/mp4" && !model.Video.FileName.EndsWith(".mp4", StringComparison.OrdinalIgnoreCase))
 54:         {
 55:             return BadRequest(new { error = "Only MP4 files are allowed" });
 56:         }
 57: 
 58:         string tempIn = null;
 59:         string tempOut = null;
 60: 
 61:         try
 62:         {
 63:             // ‚úÖ User Authentication
 64:             _logger.LogInformation("Authenticating user...");
 65:             var user = await _sb.Client.Auth.GetUser(token);
 66: 
 67:             if (user == null)
 68:             {
 69:                 return Unauthorized(new { error = "Invalid token - please login again" });
 70:             }
 71: 
 72:             var userId = user.Id;
 73:             _logger.LogInformation("User authenticated: {UserId}", userId);
 74: 
 75:             // ‚úÖ Create temp file
 76:             tempIn = Path.Combine(Path.GetTempPath(), Guid.NewGuid() + ".mp4");
 77:             _logger.LogInformation("Creating temp file: {TempPath}", tempIn);
 78: 
 79:             await using (var fs = System.IO.File.Create(tempIn))
 80:             {
 81:                 await model.Video.CopyToAsync(fs);
 82:             }
 83: 
 84:             // ‚úÖ Video Processing
 85:             _logger.LogInformation("Starting video compression...");
 86:             tempOut = await _processor.CompressAsync(tempIn);
 87:             _logger.LogInformation("Compression completed: {OutputPath}", tempOut);
 88: 
 89:             // ‚úÖ Upload to Supabase Storage
 90:             _logger.LogInformation("Uploading to Supabase Storage...");
 91:             var remoteName = $"{Guid.NewGuid()}.mp4";
 92:             var publicUrl = await UploadToSupabaseStorage(tempOut, remoteName, token, userId);
 93:             _logger.LogInformation("Storage upload completed: {Url}", publicUrl);
 94: 
 95:             // ‚úÖ Database Insert (Einfach und klar)
 96:             _logger.LogInformation("Saving post to database...");
 97: 
 98:             try
 99:             {
100:                 var post = new Post
101:                 {
102:                     UserId = Guid.Parse(userId),
103:                     MediaUrl = publicUrl,
104:                     Caption = model.Caption ?? "",
105:                     Type = "video",
106:                     Likes = 0,
107:                     Shares = 0,
108:                     CommentsCount = 0,
109:                     Category = model.Category ?? "",
110:                 };
111: 
112:                 var result = await _sb.Client.From<Post>().Insert(post);
113: 
114:                 if (result.Models?.Count > 0)
115:                 {
116:                     _logger.LogInformation($"üîç DEBUG - First inserted post MediaUrl: {result.Models[0].MediaUrl}");
117:                 }
118:                 _logger.LogInformation("Database save completed");
119:             }
120:             catch (Exception dbEx)
121:             {
122:                 _logger.LogWarning("Database save failed, but video upload was successful: {Message}", dbEx.Message);
123:                 // Continue - the video is already in storage
124:             }
125: 
126:             return Ok(new
127:             {
128:                 url = publicUrl,
129:                 userId = userId,
130:                 message = "Video uploaded successfully",
131:                 fileSize = model.Video.Length
132:             });
133:         }
134:         catch (Exception ex)
135:         {
136:             _logger.LogError(ex, "Video upload failed");
137:             return StatusCode(500, new
138:             {
139:                 error = "Video upload failed",
140:                 details = ex.Message
141:             });
142:         }
143:         finally
144:         {
145:             // ‚úÖ Cleanup
146:             TryDelete(tempIn);
147:             TryDelete(tempOut);
148:             _logger.LogInformation("Temp files cleaned up");
149:         }
150:     }
